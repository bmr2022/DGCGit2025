                                <td>@item.IndentCompleted</td>
                                <td>@item.DeptName</td>
                                <td>@item.Bomqty</td>
                                <td>@item.CC</td>
                                <td>@item.EntryId</td>
                                <td>@item.EntryDate.Split(" ")[0]</td>
                                <td>@item.Approved</td>
                                <td>@item.ApprovedbyEmpId</td>
                                <td>
                                    @{
                                        if (!string.IsNullOrEmpty(item.ApprovedDate))
                                        {
                                            string[] dateParts = item.ApprovedDate.Split(' ');
                                            if (dateParts.Length > 0)
                                            {
                                                @dateParts[0]
                                            }
                                        }
                                    }
                                </td>
                                <td>@item.CreatedByName</td>
                                <td>@item.IndentRemark</td>
                                <td>@item.MRPNO</td>
                                <td>@item.MRPEntryId</td>
                                <td>@item.MachineNo</td>
                            </tr>
                        }
                    }
                }
            </tbody>
        </table>
    }
