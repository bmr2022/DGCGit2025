<style>


    .SAGrid {
        width: 100%;
        border-collapse: collapse;
    }

    .IndentThead th {
        background-color: #127387;
        position: sticky;
        top: 0;
        height: 40px;
        z-index: 1;
    }

    .sort-icon {
        padding: 5px;
        margin: 5px;
        vertical-align: top;
    }

    .INascCol {
        font-size: 15px;
        vertical-align: bottom;
    }

    .INdescCol {
        font-size: 15px;
        vertical-align: super;
    }

    tbody td {
        /*/ Table cell styles /*/
    }
</style>

