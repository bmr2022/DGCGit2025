$('#sendToPDF').click(async function () {

    const { jsPDF } = window.jspdf;
    const doc = new jsPDF({ orientation: 'landscape', unit: 'pt', format: 'a4' });

    // Get dynamic values
    var reportTypeText = $('#ReportType').find(":selected").text();
    var reportTypeValue = $('#ReportType').val();
    var fromDate = $('#FromDate').val();
    var toDate = $('#ToDate').val();
    var WorkCenter = $('#WCID').find(":selected").text();
    var PartCode = $('#PartCode').val();
    var ItemName = $('#ItemName').val();
    var ItemGroup = $('#ItemGroup').val();
    var ItemType = $('#ItemType').val();
    var BatchNo = $('#BatchNo').val();
    var UniqueBatchNo = $('#UniqueBatchNo').val();
    var ShiftName = $('#ShiftName').val();

    // Report Title
    doc.setFontSize(16).text('PRODUCED MATERIAL TRANSFER REPORT', 200, 40);

    // Date Range
    doc.setFontSize(10).text(`For the duration From: ${fromDate}     To: ${toDate}`, 40, 58);
    doc.setFontSize(10).text(`ShiftName :${ShiftName}`, 40, 73);

    let currentY = 90;
    const headerFields = [
        { label: 'Work Center', value: (WorkCenter && WorkCenter !== '-Select-') ? WorkCenter : '' },
        { label: 'Part Code', value: PartCode || '' },
        { label: 'Item Name', value: ItemName || '' },
        { label: 'Item Group', value: ItemGroup || '' },
        { label: 'Item Type', value: ItemType || '' },
        { label: 'Batch No', value: BatchNo || '' },
        { label: 'Unique BatchNo', value: UniqueBatchNo || '' },
    ];

    // Filter out any fields with empty values
    const filled = headerFields.filter(field => field.value);

    // If there is at least one non-empty value, print the header
    if (filled.length > 0) {
        filled.forEach(field => {
            doc.setFontSize(10)
                .text(`${field.label}: ${field.value}`, 40, currentY);
            currentY += 15;
        });
    }

    // Fetch data
    let rows = [];
    try {
        var resp = await fetch('/ProductionEntryReport/GetDataForPDF');
        if (!resp.ok) throw new Error('Failed to fetch data');
        rows = await resp.json();
    } catch (error) {
        alert("Error fetching data: " + error.message);
        return;
    }

    // Declare headers and body
    let headers = [];
    let body = [];
    let foot = [];

    // Handle report type logic
    switch (reportTypeValue) {
        case 'Date Wise FG Consolidated':
            headers = [
                "Sr#", "prodEntryId", "ProdDate", "NewProdRework", "FGPartCode", "FGItemName", "Workcenter",
                "FGProdQty", "FGOKQty", "FGRejQty", "TransferToWCSTr"
            ];
                    rows.sort((a, b) => {
                        const codeA = (a.fgPartCode || '').toLowerCase();
                        const codeB = (b.fgPartCode || '').toLowerCase();
                        return codeA.localeCompare(codeB);
                    });
            // Build body
            body = rows.map((r, i) => [
                i + 1,
                r.prodEntryId ?? '',
                r.prodDate ?? '',
                r.newProdRework ?? '',
                r.fgPartCode ?? '',
                r.fgItemName ?? '',
                r.workcenter ?? '',
                r.fgProdQty ?? '',
                r.fgokQty ?? '',
                r.fgRejQty ?? '',
                r.transferToWCSTr ?? ''
            ]);

            // Calculate totals
            let totalFGProdQty = 0;
            let totalFGOKQty = 0;
            let totalFGRejQty = 0;

            rows.forEach(r => {
                totalFGProdQty += Number(r.fgProdQty) || 0;
                totalFGOKQty += Number(r.fgokQty) || 0;
                totalFGRejQty += Number(r.fgRejQty) || 0;
            });

            // Footer row with totals
            foot = [[
                '', '', '', '', '', '', 'Total:',
                totalFGProdQty.toFixed(2),
                totalFGOKQty.toFixed(2),
                totalFGRejQty.toFixed(2),
                ''
            ]];
            break;

        default:
            headers = ["Sr#", "Item Name", "Total Stock"];
            body = rows.map((r, i) => [i + 1, r.FGItemName, r.FGProdQty]);

            // Optional totals for default
            let totalDefaultQty = rows.reduce((acc, r) => acc + (Number(r.FGProdQty) || 0), 0);
            foot = [['', 'Total:', totalDefaultQty.toFixed(2)]];
            break;
    }

    // Generate table
    doc.autoTable({
        startY: currentY + 10,
        head: [headers],
        body: body,
        foot: foot,
        theme: 'grid',
        margin: { left: 40, right: 40 },
        styles: {
            fontSize: 7,
            cellPadding: 3,
            overflow: 'linebreak',
            halign: 'center',
            valign: 'middle'
        },
        headStyles: {
            fillColor: [41, 128, 185],
            textColor: 255,
            fontStyle: 'bold'
        },
        tableWidth: 'auto',
        showHead: 'everyPage'
    });

            // Step 2: Footer only on last page, aligned left, center, right
            const pageCount = doc.internal.getNumberOfPages();
            doc.setPage(pageCount); // Go to last page

            const pageWidth = doc.internal.pageSize.getWidth();
            const footerY = doc.internal.pageSize.getHeight() - 30;

            doc.setFontSize(10);

            // Left
            doc.text("Issued By: ___________________", 40, footerY);

            // Center (centered around middle of page)
            const centerText = "Q.C. MANAGER: ___________________";
            const centerX = pageWidth / 2 - (doc.getTextWidth(centerText) / 2);
            doc.text(centerText, centerX, footerY);

            // Right
            const rightText = "RECEIVE BY: ___________________";
            const rightX = pageWidth - doc.getTextWidth(rightText) - 40;
            doc.text(rightText, rightX, footerY);


    // Remarks section
            doc.setFontSize(9).text('REMARKS', 40, footerY + 35);
            doc.line(40, footerY + 40, 550, footerY + 40);

    // Save the PDF
    doc.save(`${reportTypeText.replace(/\s+/g, '_')}.pdf`);
});







        var currentSearchColumn = null;
        $(document).on("click", "th[data-columnno]", function () {
            currentSearchColumn = $(this).data('columnno');
            $("#search-box").val('').trigger('keyup'); // Reset search box and trigger the search
        });
        $("#search-box").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#divStockRegisterBody tr").filter(function () {
                if (currentSearchColumn !== null) {
                    var columnText = $(this).find('td').eq(currentSearchColumn).text().toLowerCase();
                    $(this).toggle(columnText.indexOf(value) > -1);
                } else {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1);
                }
            });
        });

        $(document).on("click", ".sortCol", function () {
            var $column = $(this).parent().closest("th");
            var colno = $(this).data('columnno');
            var currentOrder = $(this).data('sortorder');

            $('.sortCol').css('color', 'white');

            if (currentOrder === 'asc') {
                sortTable(colno, false);
                $(this).data('sortorder', 'desc');
                $(this).css('color', 'yellow'); // Visual indication of descending order
                //$(this).css('font-size', 'larger'); // Visual indication of descending order
                $(this).find('i:first').removeClass('fa-arrow-up');
                $(this).find('i:first').addClass('fa-arrow-down');
            } else {
                sortTable(colno, true);
                $(this).data('sortorder', 'asc');
                $(this).css('color', 'yellow');
                // $(this).css('font-size', 'larger'); // Visual indication of ascending order
                $(this).find('i:first').removeClass('fa-arrow-down');
                $(this).find('i:first').addClass('fa-arrow-up');
            }
        });

        function sortTable(columnIndex, ascending) {

            var table = $('#ReportTable');
            var rows = table.find('#divStockRegisterBody > tr').toArray();

            rows.sort(function (a, b) {
                var aValue = $(a).find('td').eq(columnIndex).text();
                var bValue = $(b).find('td').eq(columnIndex).text();

                var aIntValue = 0;
                var bIntValue = 0;
                if (/^\d+$/.test(aValue) && /^\d+$/.test(bValue)) {
                    aIntValue = parseInt(aValue, 10);
                    bIntValue = parseInt(bValue, 10);
                }

                if (aIntValue > 0) {
                    if (ascending) {
                        return aValue - bValue;
                    } else {
                        return bValue - aValue;
                    }
                } else {
                    if (ascending) {
                        return aValue.localeCompare(bValue);
                    } else {
                        return bValue.localeCompare(aValue);
                    }
                }
            });

            $.each(rows, function (index, row) {
                table.children('#divStockRegisterBody').append(row);
            });
        }

        function formatDate(dateStr) {
            const months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
            const [day, month, year] = dateStr.split('/');
            const monthName = months[parseInt(month) - 1];

            return `${day}/${monthName}/${year}`;
        }

