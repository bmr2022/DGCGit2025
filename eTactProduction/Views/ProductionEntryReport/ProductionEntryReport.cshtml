        function HideShow(){
             
            if ($('#ReportType').val() == 'DETAIL') {
                $('#prodschDiv').show();
                $('#prodqtyDiv').show();
                $('#okqtyDiv').show();
                $('#rejqtyDiv').show();
                $('#rmconsqty').hide();
            }
            if ($('#ReportType').val() == 'Date Wise FG Consolidated' || $('#ReportType').val() == 'Machine Wise FG Production SUmmary' || $('#ReportType').val() == 'RM Wise Consumption Summary' || $('#ReportType').val() == 'RM Wise Consumption Detail') {
                $('#prodschDiv').hide();
                $('#rmconsqty').hide();
                $('#prodqtyDiv').show();
                $('#okqtyDiv').show();
                $('#rejqtyDiv').show();
            }
            if ($('#ReportType').val() == 'RM Total Consumption') {
                $('#prodschDiv').show();
                $('#prodqtyDiv').hide();
                $('#okqtyDiv').hide();
                $('#rejqtyDiv').hide();
                $('#rmconsqty').show();
            }
            if ($('#ReportType').val() == 'BreakDown Detail' || $('#ReportType').val() == 'Machine Wise BreakDown Summary' || $('#ReportType').val() == 'Operator Detail Report' || $('#ReportType').val() == 'Operator Summary Report') {
                $('#prodschDiv').hide();
                $('#prodqtyDiv').hide();
                $('#okqtyDiv').hide();
                $('#rejqtyDiv').hide();
                $('#rmconsqty').hide();
            }
            if ($('#ReportType').val() == 'Production QC Deatil') {
                $('#prodschDiv').hide();
                $('#prodqtyDiv').show();
                $('#okqtyDiv').show();
                $('#rejqtyDiv').show();
                $('#rmconsqty').hide();
            }
            if ($('#ReportType').val() == 'Production Summary') {
               
                $('#prodqtyDiv').show();
                $('#okqtyDiv').show();
                $('#rejqtyDiv').show();
                
            }
        }
        $('#ReportType').on('change', function () {
            GetProductionReportName();
        });
        function GetProductionReportName() {
             
            const reportType = document.querySelector('[name="ReportType"]').value;
            const headingElement = document.getElementById("reportHeading");

            let headingText = '';

            switch (reportType) {
                case "DETAIL":
                    headingText = "DETAIL GRID";
                    break;
                case "Date Wise FG Consolidated":
                    headingText = "DATE WISE FG CONSOLIDATED";
                    break;
                case "Machine Wise FG Production SUmmary":
                    headingText = "MACHINE WISE FG PRODUCTION SUMMARY";
                    break;
                case "RM Wise Consumption Summary":
                    headingText = "RM WISE CONSUMPTION SUMMARY";
                    break;
                case "RM Wise Consumption Detail":
                    headingText = "RM WISE CONSUMPTION DETAIL";
                    break;
                case "RM Total Consumption":
                    headingText = "RM TOTAL CONSUMPTION";
                    break;
                case "BreakDown Detail":
                    headingText = "BREAKDOWN DETAIL";
                    break;
                case "Machine Wise BreakDown Summary":
                    headingText = "MACHINE WISE BREAKDOWN SUMMARY";
                    break;
                case "Operator Detail Report":
                    headingText = "OPERATOR DETAIL REPORT";
                    break;
                case "Operator Summary Report":
                    headingText = "OPERATOR SUMMARY REPORT";
                    break;
                case "Production QC Deatil":
                    headingText = "PRODUCTION QC DETAIL";
                    break;
                case "Production Summary":
                    headingText = "Production Summary";
                    break;

                default:
                    headingText = "ITEM DETAIL GRID";
                    break;
            }

            headingElement.innerHTML = `
                    <i class="fa-solid fa-table-list fa-lg fa-fw pr35 fa-fade" style="--fa-animation-duration: 2s; --fa-fade-opacity: 0.6;"></i>
                    ${headingText}
                `;

            
        }

        function GetProductionReport() {

            $.ajax({
                type: "POST",
                url: "@Url.Action("GetProductionEntryReport")",
                async: false,
                data: { "ReportType": $('#ReportType').val(), "FromDate": $('#FromDate').val(), "ToDate": $('#ToDate').val(), "FGPartCode": $('#FGPartCode').val(), "FGItemName": $('#FGItemName').val(), "RMPartCode": $('#RMPartCode').val(), "RMItemName": $('#RMItemName').val(), "ProdSlipNo": $('#ProdSlipNo').val(), "ProdPlanNo": $('#ProdPlanNo').val(), "ProdSchNo": $('#ProdSchNo').val(), "ReqNo": $('#ReqNo').val(), "WorkCenter": $('#WorkCenter').val(), "MachineName": $('#MachineName').val(), "OperatorName": $('#OperatorName').val(), "Process": $('#Process').val(), "ShiftName": $('#ShiftName').val() },
                success: function (result, status, error) {
                    $('#divproductionReportGrid').empty();
                    $('#divproductionReportGrid').html(result);
                    $('#totalRows').text($('#divproductionReportGrid tr').length - 1);
                    countAllTotal();
                    HideShow();
                }
            });
        }

        function GetServerDate() {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetServerDate")",
                async: false,
                success: function (result, status, error) {
                    if (result != null) {
                        var firstDayOfMonth = getFirstDayOfMonth(result);
                        $('#FromDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", firstDayOfMonth);
                        $('#ToDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", result);

                    }
                }
            });
        }

        function countAllTotal() {
             
            var rowCount = $('#divStockRegisterBody tr').length;
            var ProdSchQty = 0;
            var ProdQty = 0;
            var OkQty = 0;
            var RejQty = 0;
            var RmConsQty = 0;
            for (var i = 1; i <= rowCount; i++) {
                ProdSchQty += $('#ProdSchQty-' + i).text() == '' ? 0 : parseFloat($('#ProdSchQty-' + i).text());
                ProdQty += $('#ProdQty-' + i).text() == '' ? 0 : parseFloat($('#ProdQty-' + i).text());
                OkQty += $('#OkQty-' + i).text() == '' ? 0 : parseFloat($('#OkQty-' + i).text());
                RejQty += $('#RejQty-' + i).text() == '' ? 0 : parseFloat($('#RejQty-' + i).text());
                RmConsQty += $('#RMConsQty-' + i).text() == '' ? 0 : parseFloat($('#RMConsQty-' + i).text());
            }

            $('#TotalProdSchQty').val(isNaN(ProdSchQty) ? 0 : ProdSchQty);
            $('#TotalProdQty').val(isNaN(ProdQty) ? 0 : ProdQty);
            $('#TotalOkQty').val(isNaN(OkQty) ? 0 : OkQty);
            $('#TotalRejQty').val(isNaN(RejQty) ? 0 : RejQty);
            $('#TotalRMConsQty').val(isNaN(RmConsQty) ? 0 : RmConsQty);
        }

        function getFirstDayOfMonth(dateString) {

            const [day, month, year] = dateString.split('/');
            const currentDate = new Date(year, month - 1, day);
            currentDate.setDate(1);
            const firstDayOfMonth = `${currentDate.getDate().toString().padStart(2, '0')}/${(currentDate.getMonth() + 1).toString().padStart(2, '0')}/${currentDate.getFullYear()}`;
            return firstDayOfMonth;
        }

