                                <td>@item.StockQTy</td>
                                <td>@item.matTransferd</td>
                                <td>@item.RewQcYearCode</td>
                                @if (item.RewQcDate != null)
                                {
                                    <td>@item.RewQcDate.Split(" ")[0]</td>
                                }
                                else
                                {
                                    <td>@item.RewQcDate</td>
                                }
                                <td>@item.shiftclose</td>
                                <td>@item.ComplProd</td>
                                <td>@item.CC</td>
                                <td>@item.EntryByMachineNo</td>
                                <td>@item.ActualEmpByName</td>
                                @if (item.ActualEntryDate != null)
                                {
                                    <td>@item.ActualEntryDate.Split(" ")[0]</td>
                                }
                                else
                                {
                                    <td>@item.ActualEntryDate</td>
                                }
                                <td>@item.LastUpdatedByName</td>
                                @if (item.LastUpdationDate != null)
                                {
                                    <td>@item.LastUpdationDate.Split(" ")[0]</td>
                                }
                                else
                                {
                                    <td>@item.LastUpdationDate</td>
                                }
                                <td>@item.EntryByDesignation</td>
                                <td>@item.operatorName</td>
                                <td>@item.supervisior</td>
                            </tr>
                        }
                    }
                }

            </tbody>
        </table>
    </div>
