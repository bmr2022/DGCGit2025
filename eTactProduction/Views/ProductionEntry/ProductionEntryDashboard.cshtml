        $(document).on("click", ".sortCol", function () {
            var $column = $(this).parent().closest("th");
            var colno = $(this).data('columnno');
            var currentOrder = $(this).data('sortorder');

            $('.sortCol').css('color', 'white');
            if (currentOrder === 'asc') {
                sortTable('GateDataTable', colno, false);
                $(this).data('sortorder', 'desc');
                $(this).css('color', 'yellow');
                //$(this).css('font-size', 'larger');
                $(this).find('i:first').removeClass('fa-arrow-up');
                $(this).find('i:first').addClass('fa-arrow-down');
            } else {
                sortTable('GateDataTable', colno, true);
                $(this).data('sortorder', 'asc');
                $(this).css('color', 'yellow');
                // $(this).css('font-size', 'larger');
                $(this).find('i:first').removeClass('fa-arrow-down');
                $(this).find('i:first').addClass('fa-arrow-up');
            }
        });

        function CheckEditOrDelete(urlHref , ProdSlipNo, ProdYearCode) {
              
            event.preventDefault();
            $.ajax({
                url: '@Url.Action("CheckEditOrDelete")',
                type: "POST",
                data: { "ProdSlipNo": ProdSlipNo, "ProdYearCode": ProdYearCode },
                async: false,
                success: function (data) {
                     
                    var obj = $.parseJSON(data);
                    if(obj.Result[0] != undefined){
                        if (obj.Result[0].InProcQCSlipNo != null || obj.Result[0].QCClearingDate != null) {
                            var dateString = obj.Result[0].QCClearingDate;
                            var date = new Date(dateString);
                            var day = String(date.getDate()).padStart(2, '0');
                            var month = String(date.getMonth() + 1).padStart(2, '0'); // Months are zero-based
                            var year = date.getFullYear();
                            var formattedDate = `${day}/${month}/${year}`;
                            $.notify(" QC Completed, InProcQCSlipNo: " + obj.Result[0].InProcQCSlipNo + " and QC date: " + formattedDate + "", "error");
                            return false;
                        }
                    }
                    else{
                        var linkUrl = urlHref;
                        linkUrl = linkUrl + "&FromDate=" + $('#FromDate').val() + "&ToDate=" + $('#ToDate').val() + "&SlipNo=" + $('#SlipNo').val() + "&ItemName=" + $('#ItemName').val() + "&PartCode=" + $('#PartCode').val() + "&ProdPlanNo=" + $('#ProdPlanNo').val() + "&ReqNo=" + $('#ReqNo').val() + "&Searchbox=" + $('#search-box').val() + "&DashboardType=" + $('#DashboardType').val();
                        window.location.href = linkUrl;
                    }
                },
                error: function (response) {
                    alert(response.responseText);
                },
                failure: function (response) {
                    alert(response.responseText);
                }
            });
        }

