        function GetSearchData() {

            if ($('#DashboardType').val() == "Summary") {
                GetSummaryData();
            }
            else if ($('#DashboardType').val() == "Breakdown") {
                GetBreakdownData();
            }
            else if ($('#DashboardType').val() == "DetailWithBatchwise") {
                GetBatchwiseDetail();
            }
            else if ($('#DashboardType').val() == "Operation") {
                GetOperationData();
            }
            else if ($('#DashboardType').val() == "Scrap") {
                GetScrapData();
            }
            else {
                GetDetailData();
            }
        }

