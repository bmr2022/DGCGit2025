                                            ? res.redirectUrl
                                            : "https://" + res.redirectUrl;
                                        window.open(fullUrl, '_blank');
                                    }

                                    if (res.rawResponse) {
                                        try {
                                            const formattedJson = JSON.stringify(JSON.parse(res.rawResponse), null, 4);
                                            $('#ewayResponseBox').val(formattedJson);
                                        } catch (e) {
                                            $('#ewayResponseBox').val(res.rawResponse);
                                        }
                                        $('#PasswordAllowPopup').modal("hide");
                                    } else {
                                        $.notify('E-Way Bill generation failed.', 'error');
                                    }
                                },
                                error: function () {
                                    $.notify('An error occurred while generating the E-Way Bill.', 'error');
                                }
                            });
                        }
                        window.location.href = '/NRGP/IssueChallanOnCounter';
                    } 
                    }else {
                        alert(res.message || "Something went wrong.");
                    }
                },
