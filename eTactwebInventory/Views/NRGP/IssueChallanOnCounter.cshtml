                function submitForm(shouldPrint, shouldSend, ShouldEway, printWindow, sendWindow,GoToDashBoard,Partialview) {
            $('#ShouldPrint').val(shouldPrint); // Set hidden input
            $('#ShouldEway').val(ShouldEway); // Set hidden input
            const formData = $('form').serialize(); // Properly serialize full form
            var Email = $('#Email').val();
            var CC1 = $('#CC1').val();
            var CC2 = $('#CC2').val();
            var CC3 = $('#CC3').val();
            var ChallanNo = $('#ChallanNo').val();

            $.ajax({
                type: "POST",
                url: $('form').attr('action'),
                data: formData,
                dataType: 'json', // Expect JSON response
                success: function (res) {
                    console.log("Server response:", res);

                    if (res && res.status === "Success") {
                                 if (GoToDashBoard) {
                                                         window.location.href = '/NRGP/IssueChallanOnCounterDashboard?Flag=True';
                                   }else{
                        if (shouldPrint && res.entryId && res.yearCode && printWindow && !shouldSend) {
                            const printUrl = '/NRGP/PrintReport?EntryId=' + res.entryId + '&YearCode=' + res.yearCode;
                            printWindow.location.href = printUrl;
                        }
                                else if (Partialview) {
                                                                   window.location.href = '/NRGP/IssueChallanOnCounter?ID='+res.entryId+'&Mode=U&YC='+res.yearCode;

                                                        }
                        else if (shouldPrint && shouldSend) {
                            const printUrl = '/NRGP/PrintReport?EntryId=' + res.entryId + '&YearCode=' + res.yearCode;
                            const sendUrl = '/NRGP/SendReport?EntryId=' + res.entryId + '&YearCode=' + res.yearCode + '&EmailTo=' + Email + '&CC1=' + CC1 + '&CC2=' + CC2 + '&CC3=' + CC3 + '&Challanno=' + ChallanNo;
                            printWindow.location.href = printUrl;
                            /* sendWindow.location.href = sendUrl; */
                            $.get(sendUrl);

                        }
                        else if (ShouldEway) {
                            const ewayModel = {
                                EntryId: res.entryId,
                                InvoiceNo: res.challanNo,
                                YearCode: res.yearCode,
                                saleBillType: '', // already correct
                                customerPartCode: res.partCode,
                                transporterName: res.transporterName,
                                vehicleNo: res.vehicleNo,
                                distanceKM: String(res.distance),
                                EntrybyId: res.enteredByEmpid,
                                MachineName: res.enterByMachineName,
                                generateEway: 'Eway'
                            };
                            $.ajax({
                                url: '/NRGP/GenerateEwayBill',
                                method: 'POST',
                                contentType: 'application/json',
                                data: JSON.stringify(ewayModel),
