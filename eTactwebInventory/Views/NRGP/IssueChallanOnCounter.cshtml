        function handleInput(inputElement) {
            var inputValue = $(inputElement).val();

            inputValue = inputValue.replace(/[^0-9.]/g, '');

            var dotCount = inputValue.split('.').length - 1;
            if (dotCount > 1) {
                inputValue = inputValue.slice(0, -1);
            }

            $(inputElement).val(inputValue);
        }

