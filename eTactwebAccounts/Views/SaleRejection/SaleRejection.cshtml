                    if (result != null) {
                        var obj = $.parseJSON(result);
                        $('#SchNo').empty();
                        $('#SchNo').append('<option value="0">-Select-</option>');
                        if (obj.StatusCode != 500) {
                            $.each(obj.Result, function (index, val) {
                                $('#SchNo').append("<option value=" + val.ScheduleNo + ">" + val.ScheduleNo + "</option>");
                            });
                        }
                    }
                }
            });
