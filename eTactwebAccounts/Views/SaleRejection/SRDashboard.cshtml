                dataType: "json",
                async: false,
                success: function (result, status, error) {

                    if (result != null) {
                        var obj = $.parseJSON(result);
                        if (obj.Result != null) {
                            $('#DocumentName').empty();
                            $('#DocumentName').append('<option value="">-Select-</option>');
                            if (obj.StatusCode != 500) {
                                $.each(obj.Result.Table, function (index, val) {
                                    if (val.DocumentName != null) {
                                        $('#DocumentName').append("<option>" + val.DocumentName + "</option>");
                                    }
                                });
                            }
                        }
                    }
                }
            });
        }

        function GetSearchData() {
            var formData = {
                'summaryDetail': $('#SummaryDetail').val(),
                'custInvoiceNo': $('#CustInvoiceNo').find(':selected').text() == "-Select-" ? "" : $('#CustInvoiceNo').find(':selected').text(),
                'account_Name': $('#Account_Name').find(':selected').text() == "-Select-" ? "" : $('#Account_Name').find(':selected').text(),
                'mrnNo': $('#MrnNo').find(':selected').text() == "-Select-" ? "" : $('#MrnNo').find(':selected').text(),
                'gateNo': $('#GateNo').find(':selected').text() == "-Select-" ? "" : $('#GateNo').find(':selected').text(),
                'partCode': $('#PartCode').find(':selected').text() == "-Select-" ? "" : $('#PartCode').find(':selected').text(),
                'itemName': $('#ItemName').find(':selected').text() == "-Select-" ? "" : $('#ItemName').find(':selected').text(),
                'againstBillNo': $('#AgainstBillNo').find(':selected').text() == "-Select-" ? "" : $('#AgainstBillNo').find(':selected').text(),
                'documentName': $('#DocumentName').find(':selected').text() == "-Select-" ? "" : $('#DocumentName').find(':selected').text(),
                'voucherNo': $('#VoucherNo').find(':selected').text() == "-Select-" ? "" : $('#VoucherNo').find(':selected').text(),
                'fromDate': $('#FinFromDate').val(),
                'toDate': $('#FinToDate').val()
            };

            $.ajax({
                url: '@Url.Action("GetSearchData")',
                type: "POST",
                async: false,
                data: formData,
                success: function (data) {
                    $('#divDashboardGrid').empty().append(data);
                      GetFormRights();
                },
                error: function (response) {
                    alert(response.responseText);
                },
                failure: function (response) {
                    alert(response.responseText);
                }
            });
        }
    </script>
