@model eTactWeb.DOM.Models.SaleBillRegisterModel
<div class="table-responsive" style="max-height:300px;overflow-y:auto;">
    <table class="table table-responsive table-hover table-bordered PMRPGrid TbCss text-nowrap mb-0" id="ReportTable">
        <thead class="bg-gradient card-headerBG text-light PMRPThead" style="position:static;" id="dashRThead">
            <tr>
                <th>Action</th>
                <th>Sr#</th>
                <th data-columnno="1" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>InvoiceNo</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>InvDate </th>
                <th data-columnno="3" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>CustomerName</th>
                <th data-columnno="4" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>GSTNO</th>
                <th data-columnno="5" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>CustAddress</th>
                <th data-columnno="6" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>State</th>
                <th data-columnno="7" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>StateCode</th>
                <th data-columnno="8" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>Country</th>

                 <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>PartCode
                </th>
                <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>ItemName
                </th> 

                <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>HSNNO
                </th>
                <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Qty
                </th>
                <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Unit
                </th> 
                 <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Rate 
                </th>

                <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>DisAmt
                </th>
                <th data-columnno="10" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Amount
                </th> 
                <th data-columnno="18" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>CGSTPer</th>
                <th data-columnno="19" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>CGSTAmt</th>
                <th data-columnno="20" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>SGSTPer</th>
                <th data-columnno="21" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>SGSTAmt</th>
                <th data-columnno="22" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>IGSTPer</th>
                <th data-columnno="23" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>IGSTAmt</th>
                <th data-columnno="24" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>ExpenseAmt</th>
                 <th data-columnno="11" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>BillAmt</th>
                <th data-columnno="12" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>TaxableAmt</th>
                <th data-columnno="13" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>GSTAmount</th>
                <th data-columnno="14" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>Currency</th>
                <th data-columnno="25" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>InvAmt</th>


                <th data-columnno="15" data-sortorder="asc" class="sortCol">  <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i>
                    </span>CancelBill</th>
                <th data-columnno="16" data-sortorder="asc" class="sortCol"> <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i>
                    </span>Canceldate</th>
                <th data-columnno="16" data-sortorder="asc" class="sortCol"> <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i>
                    </span>CancelReason</th>

                <th data-columnno="17" data-sortorder="asc" class="sortCol">  <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i>
                    </span>Ewaybillno</th>

                <th data-columnno="29" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i></span>EinvGenerated</th>
                <th data-columnno="26" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>EInvNo</th>

                <th data-columnno="27" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>TypeItemServAssets</th>
                <th data-columnno="28" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>DomesticExportNEPZ</th>
                <th data-columnno="31" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>SupplyType</th>
                <th data-columnno="32" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>MachineName</th>

                <th data-columnno="33" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>EntryId</th>
                <th data-columnno="34" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>YearCode</th>
            </tr>
        </thead>
        <tbody id="divStockRegisterBody">
            @{
                var @Cntr = 0;
                if (Model.SaleBillRegisterDetail == null)
                {
                            <tr class="text-center"><td colspan="26" class="text-black-50"><strong>NO DATA FOUND</strong></td></tr>
                }
                else
                {
                    foreach (var item in Model.SaleBillRegisterDetail)
                    {
                        Cntr = Cntr + 1;

                                    <tr id="SAGridRow-@Cntr">
                                    <td>
                                <a class="table-link link-danger" onclick="DeleteRow(this.id)" id="@Cntr">
                                    <span class="fa-stack">
                                        <i class="fa fa-square fa-stack-2x"></i>
                                        <i class="fa-solid fa-trash fa-stack-1x fa-inverse"></i>
                                    </span>
                                </a>
                                    </td>
                                        <th>@Cntr</th>

                            <td style="background-color: #e09ef0;">@item.SaleBillNo</td>
                            <td>@item.SaleBillDate</td>
                            <td>@item.CustomerName</td>
                            <td>@item.GSTNO</td>
                            <td>@item.CustAddress</td>
                            <td>@item.State</td>
                            <td>@item.StateCode</td>
                            <td>@item.Country</td>
                             <td>@item.PartCode</td>
                            <td>@item.ItemName</td>
                            <td>@item.HSNNO</td>
                            <td>@item.TotalBillQty</td>
                            <td>@item.Unit</td>


                            <td>@item.Rate</td>
                            <td>@item.TotalDisAmt</td>
                            <td>@item.Amount</td>

                            <td>@item.CGSTPer</td>
                            <td>@item.CGSTAmt</td>
                            <td>@item.SGSTPer</td>
                            <td>@item.SGSTAmt</td>
                            <td>@item.IGSTPer</td>
                            <td>@item.IGSTAmt</td>
                            
                            <td>@item.ExpenseAmt</td>  
                         
                          
                            <td>@item.BillAmt</td>
                           

                             <td>@item.TaxableAmt</td>
                            <td>@item.GSTAmount</td>
                            <td>@item.Currency</td>
                            <td>@item.InvAmt</td>
                            <td>@item.CancelBill</td>
                            <td>@item.Canceldate</td>
                            <td>@item.CancelReason</td>

                            <td>@item.Ewaybillno</td>
                            <td>@item.EinvGenerated</td>
                            <td>@item.EInvNo</td>
                              
                            <td>@item.TypeItemServAssets</td>
                            <td>@item.DomesticExportNEPZ</td>
                            <td>@item.SupplyType</td>

                            <td>@item.EntryByMachineName</td>
                             
                            <td>@item.EntryId</td>
                            <td>@item.YearCode</td>



                        </tr>
                    }
                }
            }

        </tbody>
    </table>
</div>

<style>

    .sort-icon {
        padding: 5px;
        margin: 5px;
        vertical-align: top;
    }

    .ascCol {
        font-size: 15px;
        vertical-align: bottom;
    }

    .descCol {
        font-size: 15px;
        vertical-align: super;
    }
    #dashRThead th {
        background-color: #127387;
        position: sticky;
        top: 0;
        z-index: 1;
    }
</style>