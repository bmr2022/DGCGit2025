                                    <input type="hidden" asp-for="SubVoucherID" />
                                    <select class="form-control" asp-for="SubVoucherName" style="width:130px;">
                                        <option value="">BANK-RECEIPT</option>
                                        <option value="Bank-Receipt">Bank-Receipt</option>
