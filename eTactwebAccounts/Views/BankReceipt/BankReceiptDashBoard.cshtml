@model eTactWeb.DOM.Models.BankReceiptModel;
@{
    ViewData["Title"] = "Bank Receipt";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
<head>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
</head>
<div class="shadow-lg">
    <form id="SParam" method="post" asp-controller="BankReceipt" asp-action="BankReceiptDashBoard" autocomplete="off">
        <div class="card rounded border-light bg-gradient">
            <div class="card-header card-headerBG text-light bg-gradient">
                <h5>
                    Bank Receipt - <strong>Dashboard</strong>
                    <span class="d-flex float-end" style="margin-top:-2px;">
                        <a asp-action="BankReceipt" asp-controller="BankReceipt" class="btn btn-sm btn-outline-light bg-gradient" id="savebtn">
                            <i class="fa fa-plus-circle fafw fa-lg" aria-hidden="true"></i>
                            Add New
                        </a>
                    </span>
                </h5>
            </div>
            <div class="card-body">
                <div class="card overflow-auto">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-12 col-xs-12">
                                <div class="row">
                                    <div class="col">
                                        <label class="form-label">From Date</label>
                                        <input asp-for="FromDate" class="form-control" />
                                        <input type="hidden" asp-for="Mode" value="@Model.Mode" />
                                    </div>
                                    <div class="col">
                                        <label class="form-label">To Date</label>
                                        <input asp-for="ToDate" class="form-control" />
                                    </div>
                                    <input asp-for="FromDate1" type="hidden" />
                                    <input asp-for="ToDate1" type="hidden" />
                                </div>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="LedgerName" class="form-label">Ledger Name</label>
                                <select class="form-select" asp-for="LedgerName">
                                    <option value="">-Select-</option>
                                </select>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="Bank" class="form-label">Bank</label>
                                <select class="form-select" asp-for="Bank">
                                    <option value="">-Select-</option>
                                </select>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="VoucherNo" class="form-label">Voucher No</label>
                                <select class="form-select" asp-for="VoucherNo">
                                    <option value="">-Select-</option>
                                </select>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="AgainstVoucherNo" class="form-label">Against Voucher No</label>
                                <select class="form-select" asp-for="AgainstVoucherNo">
                                    <option value="">-Select-</option>
                                </select>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="SONo" class="form-label">SO No</label>
                                <select class="form-select" asp-for="SONo">
                                    <option value="">-Select-</option>
                                </select>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="AgainstBillno" class="form-label">Against Bill No</label>
                                <select class="form-select" asp-for="AgainstBillno">
                                    <option value="">-Select-</option>
                                </select>
                            </div>
                            <div class="col-xxl-2 col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-12" style="width:200px">
                                <label asp-for="DashboardType" class="form-label">Dashboard Type</label>
                                <select asp-for="DashboardType" class="form-select" onchange="GetSearchData();">
                                    <option value="" disabled>-Select-</option>
                                    <option value="Summary" >Summary</option>
                                    <option value="Detail" selected>Detail</option>
                                </select>
                            </div>
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="col-sm-3 col-md-6 col-lg-4 col-xl-2">
                                <label class="form-label">Global search</label>
                                <input type="text" class="form-control" asp-for="Searchbox" id="search-box" style="background-color:#FFFACD " placeholder="Search...">
                            </div>
                            <div class=" d-flex justify-content-center mt-3 col-sm-5 col-md-8 col-lg-6 col-xl-4" width="inherit">
                                <button class="btn btn-sm btn-outline-info" type="button" style="padding:0px;height:30px;width:100px;" onclick="GetSearchData();">
                                    <i class="fa-solid fa-magnifying-glass fa-fw fa-lg"></i>SEARCH
                                </button>
                                <button class="btn btn-sm btn-outline-success" style="padding:3px;height:30px;width:100px;margin-left:20px;" type="button" id="sendToExcel">
                                    <i class="fa-solid fa-upload fa-fw fa-lg "></i>EXCEL
                                </button>
                                <a class="btn btn-sm btn-outline-danger" style="padding:3px;height:30px;width:100px;margin-left:20px;" asp-controller="BankReceipt"
                                   asp-action="BankReceiptDashBoard">
                                    <i class="fa-solid fa-rotate fa-fw fa-lg"></i>RESET
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body pb-0 pt-0">
                <div class="progress" style="height: 4px;">
                    <div class="progress-bar progress-bar-striped bg-info" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
            <div class="card-body">
                <div class="card overflow-auto">
                    <div class="card-body" id="divDashboardGrid">
                        <partial name="_BankReceiptDashBoardGrid" />
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

@section Scripts
{
    <script>
        $(document).ready(function () {

            var FromDateBack = sessionStorage.getItem('FromDateBack');
            var ToDateBack = sessionStorage.getItem('ToDateBack');
            var LedgerNameBack = sessionStorage.getItem('LedgerNameBack');
            var VoucherNoBack = sessionStorage.getItem('VoucherNoBack');
            var AgainstVoucherRefNoBack = sessionStorage.getItem('AgainstVoucherRefNoBack');
            var AgainstVoucherNoBack = sessionStorage.getItem('AgainstVoucherNoBack');
            var GlobalSearchBack = sessionStorage.getItem('GlobalSearchBack');
            var DashboardTypeBack = sessionStorage.getItem('DashboardTypeBack');

            var currentDate = new Date();

            var firstDayOfMonth = new Date(currentDate.getFullYear(), currentDate.getMonth(), 1);

            var formatedfromdate = parseDateToFormat("@Model.FromDate1");
            var formatedTodate = parseDateToFormat("@Model.ToDate1");

            if ($('#Mode').val() != 'Search') {
                $('#FromDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", firstDayOfMonth);
                $('#ToDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", 'now');
            }
            else {
                $('#FromDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", formatedfromdate);
                $('#ToDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", formatedTodate);
            }

            var deleteMessage = '@TempData["DeleteMessage"]';

            if (deleteMessage) {
                $.notify(deleteMessage, "error");
            }
            if (FromDateBack != null && FromDateBack != '') {
                $('#FromDate').val(FromDateBack);
                $('#ToDate').val(ToDateBack);
                $('#LedgerName').val(LedgerNameBack);
                $('#VoucherNo').val(VoucherNoBack);
                $('#AgainstVoucherRefNo').val(AgainstVoucherRefNoBack);
                $('#AgainstVoucherNo').val(AgainstVoucherNoBack);
                $('#search-box').val(GlobalSearchBack);
                $('#DashboardType').val(DashboardTypeBack);
            }

            $('#LedgerName').select2();
            $('#Bank').select2();
            $('#VoucherNo').select2();
            $('#AgainstVoucherRefNo').select2();
            $('#AgainstVoucherNo').select2();
            $('#SONo').select2();
            $('#AgainstBillno').select2();
            FillLedgerInDashboard();
            FillBankInDashboard();
            FillVoucherNoInDashboard();
            FillAgainstVoucherNoInDashboard();
            FillAgainstVoucherRefNoInDashboard();
            FillAgainstBillNoInDashboard();
            FillSoNoInDashboard();
            GetFormRights();
            GetSearchData();

            sessionStorage.removeItem('FromDateBack');
            sessionStorage.removeItem('ToDateBack');
            sessionStorage.removeItem('LedgerNameBack');
            sessionStorage.removeItem('VoucherNoBack');
            sessionStorage.removeItem('AgainstVoucherRefNoBack');
            sessionStorage.removeItem('AgainstVoucherNoBack');
            sessionStorage.removeItem('GlobalSearchBack');
            sessionStorage.removeItem('DashboardTypeBack');

        });
        function GetFormRights() {

            $.ajax({
                url: "@Url.Action("GetFormRights")",
                type: "POST",
                async: false,
                success: function (data) {

                    if (data != null) {
                        var obj = $.parseJSON(data);
                        if (obj.Result.Table.length == 0) {
                            var rowCounter = $('#divDashboardGrid tr').length;
                            for (var i = 0; i < rowCounter; i++) {
                                $('#viewCursorDiv-' + i).empty();
                                $('#viewCursorDiv-' + i).append(`<a class="table-link link-success noCursor" id="viewNoCursor"">
                                                                                                        <span class="fa-stack">
                                                                                                            <i class="fa fa-square fa-stack-2x"></i>
                                                                                                            <i class="fa-solid fa-magnifying-glass fa-stack-1x fa-inverse"></i>
                                                                                                        </span>
                                                                                                    </a>`);
                            }
                            for (var i = 0; i < rowCounter; i++) {
                                $('#editCursorDiv-' + i).empty();
                                $('#editCursorDiv-' + i).append(`<a class="table-link link-primary noCursor">
                                                                                                                <span class="fa-stack">
                                                                                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                                                                                    <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                                                                                                </span>
                                                                                                            </a>`);
                            }
                            for (var i = 0; i < rowCounter; i++) {
                                $('#deleteCursorDiv-' + i).empty();
                                $('#deleteCursorDiv-' + i).append(`<a class="table-link link-danger noCursor">
                                                                                                                <span class="fa-stack">
                                                                                                                    <i class="fa fa-square fa-stack-2x"></i>
                                                                                                                    <i class="fa-solid fa-trash fa-stack-1x fa-inverse"></i>
                                                                                                                </span>
                                                                                                            </a>`);
                            }
                            $("#divDashboardGrid").css("display", "none");
                            $(".totalrows").css("display", "none");
                            $("#SParam").hide();
                        } else {

                            var optAll = obj.Result.Table[0].OptAll;
                            var optSave = obj.Result.Table[0].OptSave;
                            var optUpdate = obj.Result.Table[0].OptUpdate;
                            var optDelete = obj.Result.Table[0].OptDelete;
                            var optView = obj.Result.Table[0].OptView;
                            var rowCounter = $('#divDashboardGrid tr').length;
                            if (!optAll) {
                                if (!optSave) {

                                    $("#savebtn").css("pointer-events", "none");
                                } else {
                                    $("#savebtn").css("pointer-events", "");
                                }
                                if (!optView) {

                                    for (var i = 0; i < rowCounter; i++) {
                                        $('#viewCursorDiv-' + i).empty();
                                        $('#viewCursorDiv-' + i).append(`<a class="table-link link-success noCursor" id="viewNoCursor"">
                                                                        <span class="fa-stack">
                                                                            <i class="fa fa-square fa-stack-2x"></i>
                                                                            <i class="fa-solid fa-magnifying-glass fa-stack-1x fa-inverse"></i>
                                                                        </span>
                                                                    </a>`);
                                    }
                                }
                                if (!optUpdate) {
                                    for (var i = 0; i < rowCounter; i++) {
                                        $('#editCursorDiv-' + i).empty();
                                        $('#editCursorDiv-' + i).append(`<a class="table-link link-primary noCursor">
                                                                        <span class="fa-stack">
                                                                            <i class="fa fa-square fa-stack-2x"></i>
                                                                            <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                                                        </span>
                                                                    </a>`);
                                    }
                                }
                                if (!optDelete) {
                                    for (var i = 0; i < rowCounter; i++) {
                                        $('#deleteCursorDiv-' + i).empty();
                                        $('#deleteCursorDiv-' + i).append(`<a class="table-link link-danger noCursor">
                                                                        <span class="fa-stack">
                                                                            <i class="fa fa-square fa-stack-2x"></i>
                                                                            <i class="fa-solid fa-trash fa-stack-1x fa-inverse"></i>
                                                                        </span>
                                                                    </a>`);
                                    }
                                }
                                if (!(optUpdate || optDelete || optView)) {
                                    $("#divDashboardGrid").css("display", "none");
                                    $(".totalrows").css("display", "none");
                                    $("#SParam").hide();
                                }
                            }
                        }
                    }
                },
                error: function (errMsg) {
                    $.notify(errMsg, { position: "top center", className: "error" });
                }
            });
        }

        function GetSearchData() {


            if ($('#DashboardType').val() == "Summary") {
                GetDashBoardSummaryData();
                GetFormRights();
            }
            else {
                GetDashBoardDetailData();
                GetFormRights();
            }
        }

        function GetDashBoardSummaryData() {

            $.ajax({
                url: '@Url.Action("GetDashBoardSummaryData", "BankReceipt")',
                type: "POST",
                data: $('#SParam').serializeArray(),
                async: false,
                success: function (data) {
                    $('#divDashboardGrid').empty().append(data);
                    if ($('#gateDashboardtable tr').length < 1) {
                        var dashGrid = 0;
                        $('#totalRows').text(dashGrid);
                    } else {
                        var dashGrid = $('#gateDashboardtable tr').length;
                        $('#totalRows').text(dashGrid);
                    }
                },
                error: function (response) {
                    alert(response.responseText);
                },
                failure: function (response) {
                    alert(response.responseText);
                }
            });


        }

        function GetDashBoardDetailData() {

            $.ajax({
                url: '@Url.Action("GetDashBoardDetailData", "BankReceipt")',
                type: "POST",
                data: $('#SParam').serializeArray(),
                async: false,
                success: function (data) {

                    $('#divDashboardGrid').empty().append(data);
                    if ($('#gateDashboardtable tr').length < 1) {
                        var dashGrid = 0;
                        $('#totalRows').text(dashGrid);
                    } else {
                        var dashGrid = $('#gateDashboardtable tr').length;
                        $('#totalRows').text(dashGrid);
                    }
                },
                error: function (response) {
                    alert(response.responseText);
                },
                failure: function (response) {
                    alert(response.responseText);
                }
            });
        }

        function GetServerDate() {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetServerDate")",
                async: false,
                success: function (result, status, error) {
                    console.log(result);
                    if (result != null) {
                        $('#ToDate').datepicker({ dateFormat: 'dd/mm/yy' }).datepicker("setDate", result);
                    }
                }
            });
        }

        var currentSearchColumn = null;
        $("#search-box").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            var totalVisible = 0; // counter for visible rows

            $("#gateDashboardtable tr").filter(function () {
                if (currentSearchColumn !== null) {
                    var columnText = $(this).find('td').eq(currentSearchColumn).text().toLowerCase();
                    var isVisible = columnText.indexOf(value) > -1;
                    $(this).toggle(isVisible);
                    if (isVisible) totalVisible++;
                } else {
                    var isVisible = $(this).text().toLowerCase().indexOf(value) > -1;
                    $(this).toggle(isVisible);
                    if (isVisible) totalVisible++;
                }

            });

            // Update total rows dynamically
            $('#totalRows').text(totalVisible);
        });


        $(document).on("click", "th[data-columnno]", function () {
            currentSearchColumn = $(this).data('columnno');
            $("#search-box").val('').trigger('keyup'); // Reset search box and trigger the search
        });

        $('#sendToExcel').click(function () {
            $("#GateDataTable").table2excel({
                filename: "WorkOrderProcessDetailData.xls"
            });
        });

        $(document).on("click", ".sortCol", function () {
            var $column = $(this).parent().closest("th");
            var colno = $(this).data('columnno');
            var currentOrder = $(this).data('sortorder');

            $('.sortCol').css('color', 'white');
            if (currentOrder === 'asc') {
                sortTable('GateDataTable', colno, false);
                $(this).data('sortorder', 'desc');
                $(this).css('color', 'yellow');
                //$(this).css('font-size', 'larger');
                $(this).find('i:first').removeClass('fa-arrow-up');
                $(this).find('i:first').addClass('fa-arrow-down');
            } else {
                sortTable('GateDataTable', colno, true);
                $(this).data('sortorder', 'asc');
                $(this).css('color', 'yellow');
                // $(this).css('font-size', 'larger');
                $(this).find('i:first').removeClass('fa-arrow-down');
                $(this).find('i:first').addClass('fa-arrow-up');
            }
        });

        function confirmDelete(event) {
            // ('#SParam').serializeArray()
            event.preventDefault();
            if (confirm("Are you sure you want to Delete this entry?")) {
                var urlHref = $(event.currentTarget).attr("href");

                // Decide separator (? or &)
                var separator = urlHref.indexOf('?') === -1 ? '?' : '&';

                // Serialize ONLY SParam form
                var formData = $('#SParam').serialize();

                // Build final URL
                var linkUrl = urlHref + separator + formData;

                // Redirect
                window.location.href = linkUrl;
            }
        }

        function confirmEdit(event) {
            event.preventDefault();
            sessionStorage.setItem('FromDateBack', $('#FromDate').val());
            sessionStorage.setItem('ToDateBack', $('#ToDate').val());
            sessionStorage.setItem('LedgerNameBack', $('#LedgerName').val());
            sessionStorage.setItem('VoucherNoBack', $('#VoucherNo').val());
            sessionStorage.setItem('AgainstVoucherRefNoBack', $('#AgainstVoucherRefNo').val());
            sessionStorage.setItem('AgainstVoucherNoBack', $('#AgainstVoucherNo').val());
            sessionStorage.setItem('GlobalSearchBack', $('#search-box').val());
            sessionStorage.setItem('DashboardTypeBack', $('#DashboardType').val());

            var urlHref = $(event.currentTarget).attr("href");
            var linkUrl = urlHref;
            linkUrl = linkUrl + "&FromDate=" + $('#FromDate').val() + "&ToDate=" + $('#ToDate').val() + "&LedgerName=" + $('#LedgerName').val() + "&VoucherNo=" + $('#VoucherNo').val() + "&AgainstVoucherRefNo=" + $('#AgainstVoucherRefNo').val() + "&AgainstVoucherNo=" + $('#AgainstVoucherNo').val() + "&Searchbox=" + $('#search-box').val() + "&DashboardType=" + $('#DashboardType').val();
            window.location.href = linkUrl;
        }

        function BackButtonData(event) {

            event.preventDefault();
            sessionStorage.setItem('FromDateBack', $('#FromDate').val());
            sessionStorage.setItem('ToDateBack', $('#ToDate').val());
            sessionStorage.setItem('LedgerNameBack', $('#LedgerName').val());
            sessionStorage.setItem('VoucherNoBack', $('#VoucherNo').val());
            sessionStorage.setItem('AgainstVoucherRefNoBack', $('#AgainstVoucherRefNo').val());
            sessionStorage.setItem('AgainstVoucherNoBack', $('#AgainstVoucherNo').val());
            sessionStorage.setItem('GlobalSearchBack', $('#search-box').val());
            sessionStorage.setItem('DashboardTypeBack', $('#DashboardType').val());

            var urlHref = $(event.currentTarget).attr("href");
            var linkUrl = urlHref;
            linkUrl = linkUrl + "&FromDate=" + $('#FromDate').val() + "&ToDate=" + $('#ToDate').val() + "&LedgerName=" + $('#LedgerName').val() + "&VoucherNo=" + $('#VoucherNo').val() + "&AgainstVoucherRefNo=" + $('#AgainstVoucherRefNo').val() + "&AgainstVoucherNo=" + $('#AgainstVoucherNo').val() + "&Searchbox=" + $('#search-box').val() + "&DashboardType=" + $('#DashboardType').val();
            window.location.href = linkUrl;

        }

        $('#FromDate').change(function () {
            FillLedgerInDashboard();
            FillBankInDashboard();
            FillVoucherNoInDashboard();
            FillAgainstVoucherNoInDashboard();
            FillAgainstVoucherRefNoInDashboard();
            FillAgainstBillNoInDashboard();
            FillSoNoInDashboard();
        });

        $('#ToDate').change(function () {
            FillLedgerInDashboard();
            FillBankInDashboard();
            FillVoucherNoInDashboard();
            FillAgainstVoucherNoInDashboard();
            FillAgainstVoucherRefNoInDashboard();
            FillAgainstBillNoInDashboard();
            FillSoNoInDashboard();
        });

        function FillLedgerInDashboard() {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("FillLedgerInDashboard", "BankReceipt")',
                data: { "FromDate": $('#FromDate').val(), "ToDate": $('#ToDate').val() },
                async: false,
                dataType: "json",
                success: function (response, status, error) {
                    var obj = $.parseJSON(response);
                    $('#LedgerName').empty();
                    $('#LedgerName').append('<option value="">--Select--</option>');
                    $.each(obj.Result, function (key, val) {
                        $('#LedgerName').append('<option value = "' + val.LedgerName + '"> ' + val.LedgerName + ' </option>');
                    });
                    if ($('#Mode').val() == 'Search') {
                        $('#LedgerName').val('@Model.LedgerName');
                    }
                },
                error: function (xhr, status, error) {
                    alert('Failed to load ledger names.');
                }
            });
        }

        function FillBankInDashboard() {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("FillBankInDashboard", "BankReceipt")',
                data: { "FromDate": $('#FromDate').val(), "ToDate": $('#ToDate').val() },
                async: false,
                dataType: "json",
                success: function (response, status, error) {
                    var obj = $.parseJSON(response);
                    $('#Bank').empty();
                    $('#Bank').append('<option value="">--Select--</option>');
                    $.each(obj.Result, function (key, val) {
                        $('#Bank').append('<option value = "' + val.LedgerName + '"> ' + val.LedgerName + ' </option>');
                    });
                    if ($('#Mode').val() == 'Search') {
                        $('#Bank').val('@Model.Bank');
                    }

                },
                error: function (xhr, status, error) {
                    alert('Failed to load ledger names.');
                }
            });
        }

        function FillVoucherNoInDashboard() {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("FillVoucherNoInDashboard", "BankReceipt")',
                data: { "FromDate": $('#FromDate').val(), "ToDate": $('#ToDate').val() },
                async: false,
                dataType: "json",
                success: function (response, status, error) {
                    var obj = $.parseJSON(response);
                    $('#VoucherNo').empty();
                    $('#VoucherNo').append('<option value="">--Select--</option>');
                    $.each(obj.Result, function (key, val) {
                        $('#VoucherNo').append('<option value = "' + val.VoucherNo + '"> ' + val.VoucherNo + ' </option>');
                    });

                    if ($('#Mode').val() == 'Search') {
                        $('#VoucherNo').val('@Model.VoucherNo');
                    }

                },
                error: function (xhr, status, error) {
                    alert('Failed to load ledger names.');
                }
            });
        }

        function FillAgainstVoucherRefNoInDashboard() {
            $.ajax({
                type: 'POST',
                url: '@Url.Action("FillAgainstVoucherRefNoInDashboard", "BankReceipt")',
                data: { "FromDate": $('#FromDate').val(), "ToDate": $('#ToDate').val() },
                async: false,
                dataType: "json",
                success: function (response, status, error) {
                    var obj = $.parseJSON(response);
                    $('#AgainstVoucherRefNo').empty();
                    $('#AgainstVoucherRefNo').append('<option value="">--Select--</option>');
                    $.each(obj.Result, function (key, val) {
                        $('#AgainstVoucherRefNo').append('<option value = "' + val.againstVoucherRefNo + '"> ' + val.againstVoucherRefNo + ' </option>');
                    });

