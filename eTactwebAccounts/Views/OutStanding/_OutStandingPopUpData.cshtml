@model eTactWeb.DOM.Models.OutStandingModel

<div class="card-body table-responsive" style="max-height:300px;overflow-y:auto;">
    <table class="table table-responsive table-hover table-bordered PMRPGrid TbCss text-nowrap mb-0" id="ReportTable">
        <thead class="bg-gradient card-headerBG text-light PMRPThead" style="position:static;" id="dashRThead">
            <tr>
                <th>Sr#</th>
                <th data-columnno="1" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>
                    Voucher No
                </th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>
                    Voucher Date
                </th>
                <th data-columnno="3" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>
                    Bill/Inv No
                </th>
                <th data-columnno="4" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>
                    Dr Amt
                </th>
                <th data-columnno="5" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon"><i class="fa" aria-hidden="true"></i></span>
                    Cr Amt
                </th>
                <th data-columnno="6" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>Voucher Type</th>
                <th data-columnno="7" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>New RefNo</th>
                <th data-columnno="8" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>Doc EntryId</th>
                <th data-columnno="9" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>Acc EntryId</th>
                <th data-columnno="10" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>Acc YearCode</th>
                <th data-columnno="11" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>Against Acc EntryId</th>
                <th data-columnno="12" data-sortorder="asc" class="sortCol"><span class="sort-icon"><i class="fa"></i></span>Against Voucher YC</th>
            </tr>
        </thead>
        <tbody id="divOutStandingPopUpDatas">
            @{
                var Cntr = 0;
                if (Model.OutStandingPopUpDatas == null)
                {
                    <tr class="text-center"><td colspan="26" class="text-black-50"><strong>NO DATA FOUND</strong></td></tr>
                }
                else
                {
                    foreach (var item in Model.OutStandingPopUpDatas)
                    {
                        Cntr = Cntr + 1;
                        
                        <tr id="@Cntr" onclick="selection(this)">
                            <th>@Cntr</th>
                            <td>@item.VoucherNo</td>
                            <td>@item.VoucherDate</td>
                            <td>@item.BillOrInvoiceNo</td>
                            <td id="DrAmt-@Cntr">@item.DrAmt</td>
                            <td id="CrAmt-@Cntr">@item.CrAmt</td>
                            <td>@item.VoucherType</td>
                            <td>@item.NewRefNo</td>
                            <td>@item.DocEntryId</td>
                            <td>@item.AccEntryId</td>
                            <td>@item.AccYearCode</td>
                            <td>@item.AgainstAccEntryId</td>
                            <td>@item.AgainstAccYearCode</td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>