            var firstDayOfMonth = new Date(currentDate.getFullYear(), currentDate.getMonth(), 1);
            function formatDate(date) {
                var day = ("0" + date.getDate()).slice(-2);
                var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
                var month = monthNames[date.getMonth()];
                var year = date.getFullYear();
                return day + "/" + month + "/" + year;
            }
            //$('#FromDate').datepicker({ dateFormat: 'dd/M/yy' }).datepicker("setDate", firstDayOfMonth); // Pass the Date object directly
            //$('#ToDate').datepicker({ dateFormat: 'dd/M/yy' }).datepicker("setDate", currentDate);

            FillGroupName();
            FillPartyName();
            GetDetailsData();
            // handleReportTypeChange();

            // When ReportType changes, automatically set Group_Ledger to 'Ledger' if required
            $("#ReportType").change(function () {

                handleReportTypeChange();
            });

            // Function to handle the logic for setting 'Ledger' based on ReportType
            function handleReportTypeChange() {

                var reportType = $("#ReportType").val().trim();
                var reportTypesToCheck = ["Summary", "Detail", "IndentWithPODetail"];

                // If the ReportType matches one of the required values, select 'Ledger'
                if (reportTypesToCheck.includes(reportType)) {
                    $("#Group_Ledger").val("Ledger");
                }
            }
        });

        var selectedPartyValues = [];

        function toggleMultiSelect() {
            var isMulti = $('#multiPartySwitch').is(':checked');
            var PartynameSingleDropdown = $('#PartyNameSingle');
            var PartynameMultiDropdown = $('#PartyNameMulti');

            if (isMulti) {
                // Hide single-select and show multi-select
                PartynameSingleDropdown.hide();
                PartynameMultiDropdown.show();

                // Destroy Select2 if initialized on single-select
                if (PartynameSingleDropdown.hasClass("select2-hidden-accessible")) {
                    PartynameSingleDropdown.select2('destroy');
                }

                // Load party options
                FillPartyName();

                // Initialize multiselect plugin
                PartynameMultiDropdown.attr('multiple', 'multiple');
                PartynameMultiDropdown.multiselect({
                    templates: {
                        button: '<button type="button" class="multiselect btn btn-sm btn-outline-primary dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false"><span class="multiselect-selected-text"></span></button>',
                    },
                    onChange: function (option, checked) {
                        var value = $(option).val();
                        if (checked) {
                            if (!selectedPartyValues.includes(value)) {
                                selectedPartyValues.push(value);
                            }
                        } else {
                            selectedPartyValues = selectedPartyValues.filter(v => v !== value);
                        }
                    },
                    onSelectAll: function () {
                        selectedPartyValues = [];
                        $('#PartyNameMulti option').each(function () {
                            selectedPartyValues.push($(this).val());
                        });
                    },
                    onDeselectAll: function () {
                        selectedPartyValues = [];
                    },
                    enableFiltering: true,
                    enableCaseInsensitiveFiltering: true,
                    includeSelectAllOption: true,
                    buttonWidth: '100%',
                    maxHeight: 300,
                    nonSelectedText: 'Select Party',
                    allSelectedText: 'All selected',
                    numberDisplayed: 2,
                });

            } else {
                // Hide multi-select and show single-select
                PartynameMultiDropdown.hide();
                PartynameSingleDropdown.show();

                // Destroy multiselect if initialized
                if (PartynameMultiDropdown.data('multiselect')) {
                    PartynameMultiDropdown.multiselect('destroy');
                }

                selectedPartyValues = [];

                // Load party options
                FillPartyName();

                // Re-initialize Select2 for single-select
                PartynameSingleDropdown.select2({
                    width: '100%',
                    placeholder: 'Select Party',
                    allowClear: true
                });
            }
        }

        function selection(CurrentRow) {

            $(CurrentRow).css({ 'background-color': '#00bcd4' });
            $("#divStockRegisterBody tr").not(CurrentRow).css({ 'background-color': '' });
        }

        $('#ReportType').change(function () {
            FillGroupName();
            FillPartyName();
            selectedPartyValues = [];
            $('#PartyNameSingle').val(0);
        });

        function FillPartyName() {
            var OutStandingType = $('#ReportType').val();
            var TillDate = $('#TillDate').val();
            var isMulti = $('#multiPartySwitch').is(':checked');
            var PartynameSingleDropdown = $('#PartyNameSingle');
            var PartynameMultiDropdown = $('#PartyNameMulti');

            $.ajax({
                url: '@Url.Action("GetPartyName", "OutStanding")',
                data: {
                    "OutStandingType": OutStandingType,
                    "TillDate": TillDate
                },
                type: 'GET',
                success: function (response) {
                    var obj = $.parseJSON(response);

                    if (!isMulti) {
                        PartynameSingleDropdown.empty();
                        PartynameSingleDropdown.append('<option value="">--Select--</option>');

                        $.each(obj.Result, function (index, item) {
                            PartynameSingleDropdown.append('<option value="' + item.Account_Code + '">' + item.PartyName + '</option>');
                        });
                    } else {
                        PartynameMultiDropdown.empty();

                        $.each(obj.Result, function (index, item) {
                            PartynameMultiDropdown.append('<option value="' + item.Account_Code + '">' + item.PartyName + '</option>');
                        });

                        PartynameMultiDropdown.multiselect('rebuild');
                    }
                },
                error: function () {
                    alert('Failed to load party names.');
                }
            });
        }

        function FillGroupName() {
            var OutStandingType = $('#ReportType').val();
            var TillDate = $('#TillDate').val();

            $.ajax({
                url: '@Url.Action("GetGroupName", "OutStanding")',
                data:
                {
                    "OutStandingType": OutStandingType,

                    "TillDate": TillDate
                },
                type: 'GET',
                async: false,
                success: function (response, status, error) {

                    var PartynameDropdown = $('#GroupName');
                    var obj = $.parseJSON(response);
                    PartynameDropdown.empty();
                    PartynameDropdown.append('<option value="">--Select--</option>');

                    $.each(obj.Result, function (index, item) {
                        PartynameDropdown.append('<option value="' + item.ParentGroupName + '">' + item.ParentGroupName + '</option>');
                    });

                },
                error: function (xhr, status, error) {
                    alert('Failed to load groups.');
                }
            });
        }
        function GetDetailsData() {

            var OutStandingType = $('#ReportType').val();
            var ShowZeroBal = $('#ShowZeroBal').is(':checked');
            var TillDate = $('#TillDate').val();
            var GroupName = $('#GroupName').val();
            var AccountNameList = selectedPartyValues;
            var AccountCode = 0;

            if (AccountNameList && AccountNameList.length > 0) {
                AccountCode = 0;
            }
            else {
                AccountCode = $('#PartyNameSingle').val();
            }

            var ShowOnlyApprovedBill = $("input[name='Status']:checked").val();


            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetDetailsData", "OutStanding")',
                data: {
                    "OutStandingType": OutStandingType,

                    "TillDate": TillDate,
                    "GroupName": GroupName,
                    "AccountNameList": AccountNameList,
                    "AccountCode": AccountCode,
                    "ShowOnlyApprovedBill": ShowOnlyApprovedBill,
                    "ShowZeroBal": ShowZeroBal

                },
                success: function (response) {
                    $('#divStockMainRegister').html(response);

                    var totalRows = $('#divStockMainRegister tr').length;
                    $('#totalRows').text(totalRows);
                    countAllTotal();
                },
                error: function (xhr, status, error) {
                    console.error("Error fetching BOM tree data:", error);
                }
            });
        }

