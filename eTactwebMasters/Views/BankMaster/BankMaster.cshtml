@section Scripts {
    <script>

        $('.submitBTN').click(function (e) {
            e.preventDefault();
            // if (!ValidateTransfer_SubmitGrid()) return;
            $('form').submit();
        });

        $(document).ready(function () {
            GetFormRights();
            //RemoveOptionsfromPAC();
            $("#Entry_Date").datepicker({ minDate: 0 });
            $('#Account_Name').keyup(function () { $("#DisplayName").val(this.value).change(); });
            $('#TDS').change();
        });

        function RemoveOptionsfromPAC() {

            var selectElement = document.getElementById("ParentAccountCode");
            var optionToRemove = "6"; // value of the option to be removed
            var optionToRemove2 = "13"; // value of the option to be removed

            for (var i = 0; i < selectElement.options.length; i++) {
                if (selectElement.options[i].value === optionToRemove) {
                    selectElement.remove(i);
                    break; // Break out of the loop after removing the option
                }
            }
            for (var i = 0; i < selectElement.options.length; i++) {
                if (selectElement.options[i].value === optionToRemove2) {
                    selectElement.remove(i);
                    break; // Break out of the loop after removing the option
                }
            }
        }

        var IsSDSC = 0;
        var IsErr = 0;
        var HideShow = 0;

        $(function () {
            $("#Account_Name").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '@Url.Action("AutoComplete","BankMaster")',
                        type: "POST",
                        data: { "ColumnName": "Account_Name", "prefix": request.term },
                        success: function (data) {
                            response($.map(data,
                                function (key, value) {
                                    return key.text;
                                }));
                        },
                        error: function (response) {
                            alert(response.responseText);
                        },
                        failure: function (response) {
                            alert(response.responseText);
                        }
                    });
                },
                select: function (e, i) {
                    $("#DisplayName").val(i.item.value);
                },
                change: function (e, i) {
                    $("#DisplayName").val($("#Account_Name").val());
                },
                minLength: 1
            });
        });


        function GetFormRights() {

            $.ajax({
                url: "@Url.Action("GetFormRights", "BankMaster")",
                type: "POST",
                success: function (data) {
                    if (data != null) {

                        var obj = $.parseJSON(data);
                        if (obj.Result.Table.length == 0) {
                            $("button[type='submit']").prop("disabled", true);

                        } else {
                            console.log(obj);
                            if (obj.Result.Table[0] != undefined) {
                                var optAll = obj.Result.Table[0].OptAll;
                                var optSave = obj.Result.Table[0].OptSave;
                                var optUpdate = obj.Result.Table[0].OptUpdate;
                                var optDelete = obj.Result.Table[0].OptDelete;
                                var optView = obj.Result.Table[0].OptView;
                                if (!optAll) {
                                    if (!optSave) {
                                        $("button[type='submit']").prop("disabled", true);
                                    }
                                    if (!(optUpdate || optDelete || optView)) {
                                        $(".DashBoard")
                                            .addClass("disabled-link")
                                            .on("click", function (e) {
                                                e.preventDefault(); // Prevents clicking the link
                                            });
                                    }
                                }
                            }
                        }
                    }
                },
                error: function (errMsg) {
                    $.notify(errMsg, { position: "top center", className: "error" });
                }
            });
        }

        function AutoComplete(ID) {
            $("#" + ID).autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '@Url.Action("AutoComplete", "BankMaster")',
                        type: "POST",
                        data: { "ColumnName": ID, "prefix": request.term },
                        success: function (data) {
                            response($.map(data,
                                function (key, value) {
