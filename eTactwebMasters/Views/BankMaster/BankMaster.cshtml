                        }
                    });
                }
            });
        }

        function validateBankAccount(input) {
             
            var account = input.value.trim();
            var errorElement = document.getElementById('bankAccountError');
            var isNumeric = /^[0-9]+$/.test(account);
            var minLength = 9;
            var maxLength = 18;

            if (!isNumeric) {
                errorElement.innerText = "Account number must contain only digits."; errorElement.innerText = "Account number must contain only digits.";
                errorElement.style.display = "block";
                return false;
            }

            if (account.length < minLength || account.length > maxLength) {
                errorElement.innerText = `Account number must be between ${minLength} and ${maxLength} digits long.`;
                errorElement.style.display = "block";
                return false;
            }
            errorElement.style.display = "none";
            return true;
        }

        function validateIFSC(input) {
             
            var ifscRegex = /^[A-Z]{4}0[A-Z0-9]{6}$/;
            var messageElement = document.getElementById("ifscMessage");
            messageElement.textContent = "";

            if (!input.value) {
                messageElement.textContent = "IFSC Code cannot be empty.";
                messageElement.style.color = "red";
                return false;
            } else if (input.value.length !== 11) {
                messageElement.textContent = "IFSC Code must be 11 characters long.";
                messageElement.style.color = "red";
                return false;
            } else if (!ifscRegex.test(input.value)) {
                messageElement.textContent = "Invalid IFSC Code format.";
                messageElement.style.color = "red";
                return false;
            }
            return true;
        }

        function validateMobileNumber(mobile) {
             
            var mobilenumber = mobile.value.trim();
            var isNumeric = /^[0-9]+$/.test(mobilenumber);
            if (!isNumeric) {
                return "Account number must contain only digits.";
                mobileError.style.display = "block";
                return false;
            }
            var mobileRegex = /^[1-9]\d{9}$/;
            if (!mobileRegex.test(mobilenumber)) {
                mobileError.textContent = 'Invalid mobile number. It should be 10 digits.';
            } else {
                mobileError.textContent = '';
            }
            
        }

        function validateEmail(email) {
             
            $.ajax({
                url: '/api/validateEmail',  // The URL to your C# API endpoint
                type: 'POST',
                contentType: 'application/json',
                data: JSON.stringify(email), // Sending the email as a JSON string
                success: function (response) {
                    alert(response.message);  // Success message
                },
                error: function (xhr, status, error) {
                    alert(xhr.responseJSON.message);  // Error message
                }
            });
        }

        function validatePhoneNumber(phone) {
            var phoneInput = document.getElementById('PhoneNo');
            var phoneRegex = /^\d{3,4}-\d{6,8}$/; // Validates formats like 123-1234567 or 1234-12345678
            var phoneValue = phoneInput.value.trim();
            if (!phoneRegex.test(phoneValue)) {
                phonenoError.textContent = 'Invalid phone number. Use formats like 123-1234567 or 1234-12345678.';
            } else {
                phonenoError.textContent = '';
            }
        }

        function OnParentGroup(ID) {
            if (!ID) return false;
            else {
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("GetParentGroupDetail", "BankMaster")",
                    data: { ID: ID },
                    datatype: JSON,
                    success: function (response, status, error) {

                        var obj = $.parseJSON(response);
                        if (obj.StatusCode === 200 || obj.StatusText === "Success") {
                            $('#SubGroup').val(obj.Result.Table[0].SubGroup), $('#AccountType').val(obj.Result.Table[0].AccountType),
                                $('#MainGroup').val(obj.Result.Table[0].MainGroup), $('#SubSubGroup').val(obj.Result.Table[0].SubSubGroup),
                                $('#UnderGroup').val(obj.Result.Table[0].UnderGroup);
                            HideShow = obj.Result.Table[0].ShowHide, obj.Result.Table[0].ShowHide == 1 ? $('#divDetail').show(500) : $('#divDetail').hide(500);
                            $('#SubGroup').val() == "Sundry Debtors" || $('#SubGroup').val() == "Sundry Creditors" ? IsSDSC = 1 : IsSDSC = 0;

                            $(".is-invalid").each(function () {
                                $(this).removeClass('is-invalid');
                            });

                            $('.accordion-header button').css('background-color', '#127387');
                            $('.accordion-header small').html('');

                            console.log(IsSDSC, IsErr, HideShow);
                        }
                        else {
                            console.log(response);
                            $.notify('Something Went Wrong, Please Try Again.', "error");
                        }
                    },
                    error: function (req, status, error) {
                        console.log(req);
                    }
                });
            }
        }


        async function CheckDuplicate(n, v) {
            $.ajax({
                url: "@Url.Action("isDuplicate", "BankMaster")",
                type: "POST",
                data: { "Colval": $('#' + v).val(), "ColName": n },
                success: await function (data) {
                    if (data.result !== "0" && data.statusText === "Error") {
                        var message = 'Duplicate Entry : ' + $('#' + v).val() + ' Found For Column : ' + n;
                        $('#' + v).val('');
                        $.notify(message, { position: "top center", className: "error" });
                    }
                },
                error: function (errMsg) {
                    $.notify(errMsg, { position: "top center", className: "error" });
                }
            });
        }


        function PanNo() {
            if ($('#GSTNO').val().length == 15) {
                $('#PANNO').val($('#GSTNO').val().substring(5, 15)).focus();
            }
            else {
                $('#PANNO').val('');
            }
        }


        $('#GSTRegistered').change(function () {
            if ($(this).val() == "Y") {
                $('#GSTPartyTypes').val('CGST');
            }
            else if ($(this).val() == "N") {
                $('#GSTPartyTypes').val('IGST');
                RemoveErrorCls('GSTNO');
                RemoveErrorCls('GSTTAXTYPE');
            }
            else {
                $('#GSTPartyTypes').prop('selectedIndex', 0);
                RemoveErrorCls('GSTPartyTypes');
                RemoveErrorCls('GSTNO');
                RemoveErrorCls('GSTTAXTYPE');
            }
        });

        $('#TDS').change(function () {
            if ($(this).val() == "Y") {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("GetTDSPartyList", "BankMaster")',
                    dataType: "json",
                    success: function (data, status, error) {
                        if (data) {
                            var objResult = $.parseJSON(data);

                            if (objResult.StatusCode == 200 && objResult.StatusText == 'Success') {
                                $('#TDSPartyCategery').empty();
                                $('#TDSPartyCategery').append('<option>-Select-</option>');
                                $.each(objResult.Result, function (key, val) {
                                    $('#TDSPartyCategery').append('<option value="' + val.TDSHead + '">' + val.TDSHead + '</option>');
                                });
                            }
                            if (objResult.StatusCode == 404 && objResult.StatusText == 'Error') {
                                $('#TDSPartyCategery').empty();
                                $('#TDSPartyCategery').append('<option>-Select-</option>');
                            }
                        }
                    }
                })
            }
        });


