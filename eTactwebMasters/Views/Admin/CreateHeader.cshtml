@model System.Data.DataTable

<div class="table table-responsive-md">
    <table>
        <thead>
            <tr>
                <th>Module</th>
                <th>Main Menu</th>
                <th>Sub Menu</th>
                <th>Access</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Rows.Count; i++)
            {
                    <tr>
                        <td>
                        @Model.Rows[i]["MAINMODULE"]
                        </td>
                        <td>
                        @Model.Rows[i]["Mainmenuheading"]
                        </td>
                       @* <td>
                        @Model.Rows[i]["Submenu"]
                        </td>*@
                        <td>
                            <div class="form-check form-switch">
                                <input class="form-check-input" type="checkbox" role="switch">
                            </div>
                        </td>
                    </tr>
            }
        </tbody>
    </table>
</div>