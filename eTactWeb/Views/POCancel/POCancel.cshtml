        function GetAllowedAction() {
            $.ajax({
                type: "GET",
                url: "@Url.Action("GetAllowedAction")",
                dataType: "json",
                async: false,
                success: function (result, status, error) {

                    if (result != null) {
                        var obj = $.parseJSON(result);
                        if (obj.StatusCode == 200 && obj.StatusText == 'Success') {
                            console.log(obj);
                            var action = [];
                            for (var i = 0; i < obj.Result.Table.length; i++) {
                                action.push(obj.Result.Table[i].Action);
                            }
                            console.log(action);
                            if (action.includes('FinalLevelApproval')) {
                                //do nothing
                            } else {
                                $("#CancelationType option[value='finalApproval']").prop("disabled", true);
                            }
                            if (action.includes('AmendmentApproval')) {
