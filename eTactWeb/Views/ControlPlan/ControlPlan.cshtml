        function EditItemRows(SeqNo) {
             
            var RowCount = $('#divControlPlanGrid tr').length;
            if (EditEachItem) {
                $.notify('Cannot Edit item while AddtoGrid  Existing Item!!!', "error");
                $('#editButton-' + RowCount).prop("disabled", true);
            }
            else {
                $.ajax({
                    url: '/ControlPlan/EditItemRows',
                    type: "POST",
                    data: { SeqNo: SeqNo ,Mode:"U"},
                    success: function (data) {

                        var obj = $.parseJSON(data);
                        if (obj != null) {
                            EditEachItem = true;
                            $('#Characteristic').val(obj[0].Characteristic).trigger("change");
                            $('#EvalutionMeasurmentTechnique').val(obj[0].EvalutionMeasurmentTechnique).trigger("change");
                            $('#SpecificationFrom').val(obj[0].SpecificationFrom).trigger("change");
                            $('#SpecificationTo').val(obj[0].SpecificationTo).trigger("change");
                            $('#Operator').val(obj[0].Operator).trigger("change");
                            $('#FrequencyofTesting').val(obj[0].FrequencyofTesting).trigger("change");
                            $('#InspectionBy').val(obj[0].InspectionBy).trigger("change");
                            $('#ControlMethod').val(obj[0].ControlMethod).trigger("change");
                            $('#Remarks').val(obj[0].Remarks).trigger("change");
                            $('#RejectionPlan').val(obj[0].RejectionPlan).trigger("change");
                            $('#DrawingNo').val(obj[0].DrawingNo).trigger("change");
                            $('#SeqNo').val(obj[0].SeqNo);  
                            $('#Mode').val('U');
                            //   $('#divRoutingGrid tr#' + SeqNo).remove();

                            var rowCount = $('#divControlPlanGrid tr').length;
                            for (var i = 1; i <= rowCount + 1; i++) {
                                $('#editItemGrid-' + i).css("pointer-events", "none");
                                $('#editItemGrid-' + i).css("cursor", "not-allowed");
                            }

                            DeleteItemRow(SeqNo);
                        }
                    },
                    error: function (response) {
                        $.notify(response.responseText, "error");
                    },
                    failure: function (response) {
                        $.notify(response.responseText, "error");
                    }
                });
            }
        }
