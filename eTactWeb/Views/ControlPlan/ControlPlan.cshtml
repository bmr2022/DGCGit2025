<form asp-action="ControlPlan" enctype="multipart/form-data" asp-controller="ControlPlan" autocomplete="off" class="form-horizontal" asp-antiforgery=false>
    <div class="card rounded shadow-lg">
        <div class="card-header card-headerBG text-light justify-content-center bg-gradient">
            <h4>
                ControlPlan - @Model.Yearcode
                <span class="d-flex float-end" style="margin-top:-2px;">
                    <a asp-action="ControlPlanDashBoard" asp-route-ID="0" class="btn btn-sm btn-outline-light bg-gradient">
                        <i class="fa-solid fa-grip fa-fw fa-lg fa-beat" aria-hidden="true"></i>
                        Dashboard
                    </a>
                    <div class="row justify-content-between g-2">
                        <div class="col-auto">
                            @*<a href="#" class="btn offer-listbtn hvr-sweep-to-right btn-sm btn-secondary" onclick="back();">*@
                            <a href="#" class=" btn btn-primary btn-sm" onclick="back();">
                                <i class="fa fa-chevron-circle-left fa-fw fa-lg" aria-hidden="true"></i>

                                BACK
                            </a>
                        </div>
                        @if (Model.Mode != "V" && Model.Mode != "SOC" && Model.Mode != "POC")
                        {
                            <div class="col-auto">
                                @*  <a class="btn btn-sm btn-outline-danger" onclick="location.href=$('form')[0].action">*@
                                <a class="btn btn-secondary btn-sm" onclick="location.href=$('form')[0].action">
                                    @* <i class="fa-solid fa-grip fa-lg fa-fw fa-beat" style="--fa-animation-duration: 2s;"></i>*@
                                    <i class="fa fa-refresh fa-fw fa-lg" aria-hidden="true"></i>
                                    REFRESH
                                </a>
                            </div>
                            @if (Model.Mode == "U")
                            {
                                @*Model.Mode = "U";*@
                                <div class="col-auto">
                                    @* <button type="submit" class="btn btn-sm btn-outline-warning">*@
                                    <input type="hidden" id="Mode" name="Mode" asp-for="Mode" value="U" />
                                    <button type="submit" class="btn btn-sm btn-outline-light bg-gradient" id="updateBtnHeader">
                                        <i class="fa fa-pencil-square fa-fw fa-lg" aria-hidden="true"></i>
                                        UPDATE
                                    </button>
                                </div>
                            }

                            else
                            {
                                <div class="col-auto">
                                    @*   <button type="submit" class="btn btn-sm btn-outline-primary">*@
                                    <button type="submit" class="btn btn-primary btn-sm" id="submitBTN">
                                        <i class="fa-solid fa-chevron-circle-right fa-fw fa-lg"></i>
                                        SUBMIT
                                    </button>
                                </div>
                            }
                        }
                    </div>
                </span>
            </h4>
        </div>
        <div class="card-body bg-light">
            <div class="accordion shadow-lg" id="accordionAccountMaster">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="B_ItemCategoryDetail">
                        <button class="accordion-button text-light pnl1BG bg-gradient" type="button" data-bs-toggle="collapse" data-bs-target="#B_AccountDetail" aria-expanded="true" aria-controls="B_AccountDetail">
                            <strong><i class="fa fa-fw pr35 fa-book fa-lg" aria-hidden="true"></i>ControlPlan</strong>
                          
                        </button>
                    </h2>
                    <div id="B_AccountDetail" class="accordion-collapse collapse pnl1 bg-gradient show" aria-labelledby="H_AccountDetail">
                        <div class="accordion-body">
                            <input asp-for="EntryByMachine" type="hidden" class="form-control" value="@Environment.MachineName"  />
                            <input asp-for="Mode" type="hidden" class="form-control" value="@Model.Mode"  />
                            <input type="hidden" asp-for="SeqNo" value="0" />
                            <input asp-for="CntPlanEntryDate" type="hidden" class="form-control" value="@Model.CntPlanEntryDate" />
                            <input asp-for="CntPlanYearCode" type="hidden" class="form-control" value="@Model.CntPlanYearCode" />
                            <input asp-for="CC" type="hidden" class="form-control" value="@Model.CC" />
                            <input asp-for="ActualEntryDate" type="hidden" class="form-control" value="@Model.ActualEntryDate" />
                            <input asp-for="ActualEntryBy" type="hidden" class="form-control" value="@Model.ActualEntryBy" />
                            <div class="row g-3">
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:70px;">
                                    <label asp-for="CntPlanEntryId" class="form-label">Entry Id</label>
                                    <input asp-for="CntPlanEntryId" class="form-control" readonly/>
                                </div>
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <label asp-for="Entry_Date" class="form-label">Entry Date</label>
                                    <input type="text" id="Entry_Date" name="Entry_Date" class="form-control"
                                           value="@(Model.Entry_Date != null ? Convert.ToDateTime(Model.Entry_Date).ToString("dd/MM/yyyy") : "")" />

                                </div>
                                    <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:90px;">
                                        <label asp-for="Yearcode" class="form-label">Yearcode</label>
                                        <input asp-for="Yearcode" class="form-control" readonly />
                                    </div>

                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:70px;">
                                    <label asp-for="Control_PlanNo" class="form-label">Plan No</label>
                                    <input asp-for="Control_PlanNo" class="form-control"  />
                                </div>
                               <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:90px;">
                                    <label asp-for="RevNo" class="form-label" style="color:red">Rev No*</label>
                                    <input asp-for="RevNo" class="form-control" value="@(Model.Mode == "U" ? Model.RevNo : "1")" />
                                </div>
                               <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                    <label asp-for="EffectiveDate" class="form-label">Effective Date</label>
                                    <input type="text" id="EffectiveDate" name="EffectiveDate" class="form-control"
                                           value="@(Model.EffectiveDate != null ? Convert.ToDateTime(Model.EffectiveDate).ToString("dd/MM/yyyy") : "")" />
                               </div>
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <label asp-for="PartCode" class="form-label" style="color:red">Part Code*</label>
                                    <select class="form-control" id="PartCode" asp-for="PartCode" onchange="checkItemOrPart()">
                                        <option value="">-Select-</option>
                                        @if (Model.Mode == "U" || Model.Mode == "V")
                                        {

                                            <option value="@Model.ItemCode" selected>@Model.PartCode</option>
                                        }
                                    </select>
                                </div>
                                

                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12">
                                    <label asp-for="ItemName" class="form-label" style="color:red"> Item Name*</label>
                                    <select class="form-control"  id="ItemName" asp-for="ItemName" onchange="checkItemOrPart()">
                                        <option value="">-Select-</option>
                                        @if (Model.Mode == "U" || Model.Mode == "V")
                                        {
                                            <option value="@Model.ItemCode" selected>@Model.ItemName</option>
                                        }
                                    </select>
                                </div>

                                <input type="hidden" id="ItemCode" name="ItemCode" value="@Model.ItemCode" />
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:120px;">
                                    <label class="form-label">Copy This Plan</label>
                                    <div class="form-check form-switch" style="margin-top:5px;">
                                        <input asp-for="CopyControlPlan" class="form-check-input" type="checkbox" role="switch" onchange="CopyControlPlanFunc(this);" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h2 class="accordion-header" id="H_SAGrid">
