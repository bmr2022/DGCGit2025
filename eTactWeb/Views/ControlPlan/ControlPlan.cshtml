        $('#btnImport').click(function () {
            var fileInput = document.getElementById('importFile');
            var file = fileInput.files[0];

            if (!file) {
                $.notify("Please choose a file to import.", "error");
                return;
            }

            var reader = new FileReader();

            reader.onload = function (e) {
                var data = e.target.result;
                var workbook = XLSX.read(data, { type: 'binary' });
                var firstSheet = workbook.SheetNames[0];
                var sheetData = XLSX.utils.sheet_to_json(workbook.Sheets[firstSheet]);

                if (sheetData.length === 0) {
                    $.notify("No data found in the file.", "error");
                    return;
                }

                var allItems = sheetData.map(row => ({
                    SeqNo: 0,
                    Mode: "",
                    CntPlanEntryDate: row.CntPlanEntryDate || null,
                    CntPlanEntryId: row.CntPlanEntryId || 0,
                    CntPlanYearCode: row.CntPlanYearCode || 0,
                    Characteristic: row.Characteristic || "",
                    EvalutionMeasurmentTechnique: row.EvalutionMeasurmentTechnique || "",
                    SpecificationFrom: row.SpecificationFrom || "",
                    Operator: row.Operator || "",
                    SpecificationTo: row.SpecificationTo || "",
                    FrequencyofTesting: row.FrequencyofTesting || "",
                    InspectionBy: row.InspectionBy || "",
                    ControlMethod: row.ControlMethod || "",
                    RejectionPlan: row.RejectionPlan || "",
                    Remarks: row.Remarks || "",
                    ItemimagePath: row.ItemimagePath || "",
                    DrawingNo: row.DrawingNo || "",
                    DrawingNoImagePath: row.DrawingNoImagePath || "",
                    ImageURL: row.ImageURL || "",
                    Control_PlanNo: row.Control_PlanNo || "",
                    RevNo: row.RevNo || "",
                    ForInOutInprocess: row.ForInOutInprocess || "",
                    ItemCode: row.ItemCode || 0,
                    PartCode: row.PartCode || "",
                    ItemName: row.ItemName || "",
                    AccountCode: row.AccountCode || 0,
                    EngApprovedBy: row.EngApprovedBy || 0,
                    ApprovedBy: row.ApprovedBy || 0,
                    UId: row.UId || 0,
                    EntryByMachine: row.EntryByMachine || "",
                    ActualEntryBy: row.ActualEntryBy || 0,
                    ActualEntryByName: row.ActualEntryByName || "",
                    ActualEntryDate: row.ActualEntryDate || null,
                    LastUpdatedBy: row.LastUpdatedBy || 0,
                    LastUpdatedByName: row.LastUpdatedByName || "",
                    LastUpdationDate: row.LastUpdationDate || null,
                    CC: row.CC || null
                }));

                console.log("Sending data to server:", JSON.stringify(allItems));

                $.ajax({
                    url: '@Url.Action("ImportControlPlanDetails")',
                    type: 'POST',
                    data: JSON.stringify(allItems),
                    contentType: 'application/json; charset=utf-8',
                    dataType: 'html',
                    success: function (data) {
                        $('#divControlPlanGrid').html(data);
                        $.notify("Import complete.", "success");
                    },
                    error: function (xhr) {
                        $.notify("Import failed: " + xhr.responseText, "error");
                        console.error("Import failed:", xhr.responseText);
                    }
                });
            };

            reader.readAsBinaryString(file);
        });
        function ExportItems() {
            // Clone header and grid
            let headerRowClone = $("#headerRow").clone(true);
            let tableBodyClone = $("#divControlPlanGrid").clone(true);

            let removeIndices = [];

            // Identify columns to remove and clean header text
            $("#headerRow th").each(function (index) {
                let text = $(this).text().trim();

                // Remove "Action" and "Seq No" columns
                if (text === "Action" || text === "Seq No") {
                    removeIndices.push(index);
                    $(this).remove();
                } else {
                    // Remove spaces from header text
                    $(this).text(text.replace(/\s+/g, ""));
                }
            });

            // Remove matching <td> cells in all rows
            $("#divControlPlanGrid tr").each(function () {
                $(this).find("td").each(function (index) {
                    if (removeIndices.includes(index)) {
                        $(this).remove();
                    }
                });
            });

            // Export to Excel
            let table = $("#itemtable")[0];
            TableToExcel.convert(table, {
                name: `ImportItemsFile.xlsx`,
                sheet: {
                    name: 'ImportItemsFile'
                }
            });

            // Restore original DOM
            $("#headerRow").replaceWith(headerRowClone);
            $("#divControlPlanGrid").replaceWith(tableBodyClone);
        }


