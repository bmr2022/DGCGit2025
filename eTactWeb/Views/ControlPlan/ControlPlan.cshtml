        var EditEachItem = false;

        $('.GridButton').click(function (e) {
             
            e.preventDefault();
            var imageUrl = $('#hdnImageURL').val();
            var itemImageUrl = $('#hdnItemImageURL').val();
            EditEachItem = false;

            function isNullOrEmpty(val) {
                return val === undefined || val === null || val.trim() === "";
            }

            // Mandatory Field Validation
            if (isNullOrEmpty($('#Characteristic').val())) {
                $.notify("Characteristic is required.", "error");
                $('#Characteristic').focus();
                return false;
            }
            if (isNullOrEmpty($('#EvalutionMeasurmentTechnique').val())) {
                $.notify("Evaluation Measurement Technique is required.", "error");
                $('#EvalutionMeasurmentTechnique').focus();
                return false;
            }
            if (isNullOrEmpty($('#ControlMethod').val())) {
                $.notify("Control Method is required.", "error");
                $('#ControlMethod').focus();
                return false;
            }
            if (isNullOrEmpty($('#SpecificationFrom').val())) {
                $.notify("Specification From is required.", "error");
                $('#SpecificationFrom').focus();
                return false;
            }

            // Calculate highest SeqNo in grid
            var seqnoarray = [];
            $('#divControlPlanGrid tr').each(function (index, element) {
                var currentRow = $(element);
                var seqnoText = currentRow.find('td[id^=SeqNo]').text();
                if (!isNullOrEmpty(seqnoText) && !isNaN(parseInt(seqnoText))) {
                    seqnoarray.push(parseInt(seqnoText));
                }
            });

            var highestValue = 0;
            if (seqnoarray.length > 0) {
                highestValue = Math.max(...seqnoarray);
            }
            var seqNo = $('#SeqNo').val();
            var mode = $('#Mode').val();

            if (!seqNo || seqNo === "0" || seqNo === "") {
                mode = ""; // reset mode to insert for new entry
                $('#Mode').val(""); // also reset hidden input to keep UI consistent
            }
            // Determine SeqNo to use
            // If mode is 'U' (update), keep current SeqNo; else assign new SeqNo = highest + 1
            var finalSeqNo = ($('#Mode').val() === "U") ? $('#SeqNo').val() : (highestValue + 1);
            $('#SeqNo').val(finalSeqNo); // update hidden field or input if needed

            var ItemData = {
                "Characteristic": $('#Characteristic').val(),
                "EvalutionMeasurmentTechnique": $('#EvalutionMeasurmentTechnique').val(),
                "SpecificationFrom": $('#SpecificationFrom').val(),
                "SpecificationTo": $('#SpecificationTo').val(),
                "Operator": $('#Operator').val(),
                "OperatorText": $('#Operator').find(':selected').text() == "-Select-" ? "" : $('#Operator').find(':selected').text(),
                "FrequencyofTesting": $('#FrequencyofTesting').val(),
                "InspectionBy": $('#InspectionBy').val(),
                "InspectionByText": $('#InspectionBy').find(':selected').text() == "-Select-" ? "" : $('#InspectionBy').find(':selected').text(),
                "ControlMethod": $('#ControlMethod').val(),
                "ControlMethodText": $('#ControlMethod').find(':selected').text() == "-Select-" ? "" : $('#ControlMethod').find(':selected').text(),
                "Remarks": $('#Remarks').val(),
                "RejectionPlan": $('#RejectionPlan').val(),
                "RejectionPlanText": $('#RejectionPlan').find(':selected').text() == "-Select-" ? "" : $('#RejectionPlan').find(':selected').text(),
                "DrawingNo": $('#DrawingNo').val(),
                "CntPlanEntryDate": $('#CntPlanEntryDate').val(),
                "CntPlanYearCode": $('#CntPlanYearCode').val(),
                "CntPlanEntryId": $('#CntPlanEntryId').val(),
                "ImageURL": imageUrl,
                "ItemImageURL": itemImageUrl,
                "Mode": mode,
                "SeqNo": finalSeqNo
            };

            console.log(ItemData);

            $.ajax({
                url: "@Url.Action("AddControlPlanDetail")",
                type: "POST",
                data: ItemData,
                beforeSend: function (xhr) {
                    console.log(1);
                },
                success: function (data, status, xhr) {
                    console.log(2);
                    $('#RoundOff').prop('selectedIndex', 0);
                    if (data === "Duplicate" && (xhr.statusCode === 207 || xhr.responseText === "Duplicate")) {
                        $.notify('Duplicate Process Not Allowed.', "info");
                        $('.ItemBtn').html('<span role="status"><i class="fa-solid fa-angles-down fa-fw fa-lg"></i> ADD TO GRID</span>').prop("disabled", false);
                    }
                    else {
                        $('#divControlPlanGrid').html(data);

                        // If it's a new add (CntPlanEntryId is 0 or empty), clear mode; else keep mode
                        if ($('#CntPlanEntryId').val() === "0" || $('#CntPlanEntryId').val() === "") {
                            $('#Mode').val("");
                        }

                        $('#SeqNo').val("");
                        $.notify('Process Added To Grid', "success");
                    }
                    ClearItems();
                },
                error: function (result, status, xhr) {
                    $.notify(result.responseText, "error");
                },
                failure: function (result, status, xhr) {
                    alert(result.responseText);
                },
                complete: function () {
                    console.log(5);
                }
            }).done(function () {
                console.log(3);
            }).fail(function () {
                alert("error");
            }).always(function () {
                console.log(4);
            });
        });

