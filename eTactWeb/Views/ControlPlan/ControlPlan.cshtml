                                    <input asp-for="ControlMethod" class="form-control" />
                                </div>
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:140px;">
                                    <label asp-for="SpecificationFrom" class="form-label" style="color:red">Specification From*</label>
                                    <input asp-for="SpecificationFrom" class="form-control" />
                                </div>
                                 
                                 
                               
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:90px;">
                                    <label asp-for="Operator" class="form-label">Operator</label>
                                    <select class="form-control" id="Operator" asp-for="Operator" >
                                        <option value="">-Select-</option>
