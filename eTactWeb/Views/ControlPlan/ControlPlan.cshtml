                                    </select>
                                </div>
                               
                               
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:140px;">
                                    <label asp-for="Remarks" class="form-label">Remarks</label>
                                    <input asp-for="Remarks" class="form-control" />
                                </div>
                                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 col-xs-12" style="width:140px">
                                    <label asp-for="RejectionPlan" class="form-label">RejectionPlan</label>
                                    <select class="form-control" id="RejectionPlan" asp-for="RejectionPlan" style="width:130px;">
                                        <option value="@Model.RejectionPlan">-Select-</option>
                                    </select>
                                </div>
                               
                          
                                
                                
                                
