										<label asp-for="PartName" class="form-label" style="color:red"> Part Name*</label>
										<select class="form-control" id="PartName" asp-for="PartName">
											<option value="">-Select-</option>
											@if (Model.Mode == "U" || Model.Mode == "V")
											{
