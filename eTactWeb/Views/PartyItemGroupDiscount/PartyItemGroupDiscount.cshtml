		function SetPartCodeItemName(ID) {
			if (!ID) return;
			else {

				var v = $('#' + ID).select2('val');
				if (ID == "CategoryCode") {
					$("#CategoryName").val(v).trigger("change");
				}

				if (ID == "CategoryName") {
