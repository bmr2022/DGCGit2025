        document.addEventListener("DOMContentLoaded", function () {
            document.querySelectorAll(".UpdateButton").forEach(button => {
                button.addEventListener("click", function (event) {
                    // Find the closest button that has the `data-type` attribute
                    let parentButton = this.closest("button");
                    if (parentButton) {
                        let typeValue = parentButton.getAttribute("data-type");
                        console.log("Selected Type:", typeValue); // Debugging

                        // Store the type value in a hidden input field
                        let hiddenInput = document.getElementById("TypeInput");
                        if (hiddenInput) {
                            hiddenInput.value = typeValue;
                        }

                        // Submit the form automatically
                        document.getElementById("UpdateForm").submit();
                    }
                });
            });
        });


