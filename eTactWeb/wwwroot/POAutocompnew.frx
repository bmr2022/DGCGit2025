    private void Text240_BeforePrint(object sender, EventArgs e)
    {
      decimal  value1 = ToDecimal(Report.GetColumnValue(&quot;ReportData.NetAmount&quot;));
    
      if (value1%1 == 0 )
        Text240.Text = ToString(FormatNumber(value1,0))  ;
     
      
    }
    
    private void Text324_BeforePrint(object sender, EventArgs e)
    {
     
        string poForValue = (string)Report.GetColumnValue(&quot;ReportData.POFor&quot;);
       
      if (poForValue == &quot;For Purchase&quot;)
      {
        Text324.Text = &quot;Purchase order&quot;;
      }
      else if (poForValue == &quot;For JobWork&quot;)
      {
        Text324.Text = &quot;Jobwork order&quot;;
      }
      else if (poForValue == &quot;For Service&quot;)
      {
        Text324.Text = &quot;Service order&quot;;
      }
      else
      {
        // Optional: Handle cases where none of the conditions are met
        Text324.Text = &quot;Unknown order type&quot;;
      }
       
     
    }

    private void Text332_BeforePrint(object sender, EventArgs e)
    {
      decimal  value2 = ToDecimal(Report.GetColumnValue(&quot;ReportData.BasicAmount&quot;));
      decimal  value3 = ToDecimal(Report.GetColumnValue(&quot;ReportData.NetAmount&quot;));
      decimal value4 = value2 + value3-value2;
    
     
        Text332.Text = value4.ToString(&quot;F2&quot;);
    }

    

    private void Text355_BeforePrint(object sender, EventArgs e)
    {
      object value = Report.GetColumnValue(&quot;ReportData.AmmEffDate&quot;);
      string Ammeffdate = (value == null || value == DBNull.Value) ? null : value.ToString();

//      if (string.IsNullOrEmpty(Ammeffdate) || 
//        (Ammeffdate.Contains(&quot;1900-01-01&quot;))) // Check if string contains &quot;01-01-1900&quot;
//      {
//        Text355.Text = &quot;&quot;;
//      }
//      else
//      {
//        Text355.Text = Ammeffdate;
//      }
      DateTime dateValue;

      if (value is DateTime)
      {
        dateValue = (DateTime)value;
      }
      else if (!DateTime.TryParse(value.ToString(), out dateValue))
      {
        Text355.Text = &quot;&quot;;
        return;
      }

      // Check for the default SQL date 1900-01-01
      if (dateValue == new DateTime(1900, 1, 1))
      {
        Text355.Text = &quot;&quot;;
      }
      else
      {
        // Optional: format the date string as needed
        Text355.Text = dateValue.ToString(&quot;dd-MM-yyyy&quot;); // Or any format you need
      }
      
    
    }
  }
}
