    private void Text51_BeforePrint(object sender, EventArgs e)
    {
     
    }
    public decimal GetTotalStockDiffSum()
    {
      return scriptTotalStockDiffSum;
    }
    

    private void Text52_BeforePrint(object sender, EventArgs e)
    {
      decimal  Singlereqqty1 = ToDecimal(Report.GetColumnValue(&quot;SPBOMPrintReportForDirectBOMStockShortExcess.NetReqQty&quot;));
      decimal  Totreqqty1 = ToDecimal(Report.GetColumnValue(&quot;SPBOMPrintReportForDirectBOMStockShortExcess.TotalReqQty&quot;));
    
      Text52.Text=(Totreqqty1/Singlereqqty1).ToString();
    }

    private void Text54_BeforePrint(object sender, EventArgs e)
    {
      decimal  totstock = ToDecimal(Report.GetColumnValue(&quot;SPBOMPrintReportForDirectBOMStockShortExcess.StoreStock&quot;));
      decimal  reqqty = ToDecimal(Report.GetColumnValue(&quot;SPBOMPrintReportForDirectBOMStockShortExcess.TotalReqQty&quot;));
      decimal difference = totstock - reqqty;
      Text54.Text = (totstock - reqqty).ToString();
      scriptTotalStockDiffSum += difference;
    }
  }
}
