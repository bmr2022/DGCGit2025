@model eTactWeb.DOM.Models.MRNRegisterModel
<div class="table-responsive" style="max-height:300px;overflow-y:auto;">
    <table class="table table-responsive table-hover table-bordered PMRPGrid TbCss text-nowrap mb-0" id="ReportTable">
        <thead class="bg-gradient card-headerBG text-light PMRPThead" style="position:static;" id="dashRThead">
            <tr>
                <th>Action</th>
                <th>Sr#</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>MRN No
                </th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>MRN Date
                </th>

                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>GateNo</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>GDate</th> 
                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Vendor Name</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Invoice No</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Invoice Date</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Document Name
                </th>
                    <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Total Bill Qty</th>

                <th data-columnno="2" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Total Rec Qty </th>
                <th data-columnno="3" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Short Exc Qty</th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>FOC
                </th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Currency
                </th>
                <th data-columnno="3" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Rec In Store
                </th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>MRNType
                </th>
                <th data-columnno="3" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>MRN QC Compl
                </th>
                <th data-columnno="2" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>NeedTochkQC
                </th>
                <th data-columnno="3" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>VendAddress
                </th>

                <th data-columnno="4" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Entered By </th>

                <th data-columnno="4" data-sortorder="asc" class="sortCol">
                    <span style="padding:5px;margin:5px" class="sort-icon">
                        <i class="fa" aria-hidden="true"></i>
                    </span>Entered Date
                </th>
                <th data-columnno="5" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Updated By </th>
                <th data-columnno="6" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Last Updation Date</th>
                <th data-columnno="7" data-sortorder="asc" class="sortCol"><span style="padding:5px;margin:5px" class="sort-icon">
                    <i class="fa" aria-hidden="true"></i></span>Entry By Comp</th>
            </tr>
        </thead>
        <tbody id="divStockRegisterBody">
            @{
                var @Cntr = 0;
                if (Model.MRNRegisterDetail == null)
                {
                            <tr class="text-center"><td colspan="26" class="text-black-50"><strong>NO DATA FOUND</strong></td></tr>
                }
                else
                {
                    foreach (var item in Model.MRNRegisterDetail)
                    {
                        Cntr = Cntr + 1;

                                    <tr id="SAGridRow-@Cntr">
                                    <td>
                                <a class="table-link link-danger" onclick="DeleteRow(this.id)" id="@Cntr">
                                    <span class="fa-stack">
                                        <i class="fa fa-square fa-stack-2x"></i>
                                        <i class="fa-solid fa-trash fa-stack-1x fa-inverse"></i>
                                    </span>
                                </a>
                            <th>@Cntr</th>
                            <td>@item.MRNNo </td>
                            <td>@item.MRNDate</td>

                            <td>@item.GateNo </td>
                            <td>@item.GDate</td>
                             

                            <td>@item.VendorName</td>
                            <td>@item.InvoiceNo</td>
                            <td>@item.InvoiceDate</td>
                            <td>@item.DocNo</td>
                            <td>@item.BillQty</td>
                            <td>@item.RecQty</td>
                            <td>@item.ShortExcessQty</td>
                            <td>@item.FOC</td>
                            <td>@item.Currency</td>
                            <td>@item.RecInStore</td>
                            <td>@item.MRNType</td>
                            <td>@item.MRNQCCompleted</td>
                            <td>@item.NeedTochkQC</td>
                            <td>@item.VendAddress</td>


                             <td>@item.ActualEntryByEmp</td>
                            <td>@item.ActualEntryDate</td>

                            <td>@item.LastUpdatedby</td>
                            <td>@item.LastUpdatedDate</td>

                            <td>@item.EntryByMachineName</td>


                        </tr>
                    }
                }
            }

        </tbody>
    </table>
</div>

<style>

    .sort-icon {
        padding: 5px;
        margin: 5px;
        vertical-align: top;
    }

    .ascCol {
        font-size: 15px;
        vertical-align: bottom;
    }

    .descCol {
        font-size: 15px;
        vertical-align: super;
    }
    #dashRThead th {
        background-color: #127387;
        position: sticky;
        top: 0;
        z-index: 1;
    }
</style>